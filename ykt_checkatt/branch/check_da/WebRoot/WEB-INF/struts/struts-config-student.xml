<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
        "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
    <form-beans>

        <form-bean name="termForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="termId" type="java.lang.String"/>
            <form-property name="termName" type="java.lang.String"/>
            <form-property name="beginDate" type="java.lang.String"/>
            <form-property name="endDate" type="java.lang.String"/>
        </form-bean>

        <form-bean name="roomForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="roomId" type="java.lang.String"/>
            <form-property name="schoolArea" type="java.lang.String"/>
            <form-property name="build" type="java.lang.String"/>
            <form-property name="roomName" type="java.lang.String"/>
            <form-property name="deviceId" type="java.lang.Integer"/>
        </form-bean>

        <form-bean name="courseImpForm" type="org.king.upload.web.form.UpLoadForm"/>

        <form-bean name="classdevForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="schoolarea" type="java.lang.String"/>
            <form-property name="build" type="java.lang.String"/>
            <form-property name="roomname" type="java.lang.String"/>
            <form-property name="roomid" type="java.lang.String"/>
            <form-property name="unbinddevice" type="java.lang.String"/>
            <form-property name="binddevice" type="java.lang.String"/>
        </form-bean>

        <form-bean name="roomImpForm" type="org.king.upload.web.form.UpLoadForm"/>

        <form-bean name="classTimesForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="classNum" type="java.lang.String"/>
            <form-property name="beginTime" type="java.lang.String"/>
            <form-property name="endTime" type="java.lang.String"/>
        </form-bean>

        <form-bean name="todaystuchecksearchForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="myclass" type="java.lang.String"/>
            <form-property name="lecture" type="java.lang.String"/>
            <form-property name="endTime" type="java.lang.String"/>
        </form-bean>

        <form-bean name="CourseplanForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="planId" type="java.lang.String"/>
            <form-property name="gardno" type="java.lang.String"/>
            <form-property name="areaCode" type="java.lang.String"/>
            <form-property name="areaName" type="java.lang.String"/>
            <form-property name="schoolArea" type="java.lang.String"/>
            <form-property name="classname" type="java.lang.String"/>
            <form-property name="build" type="java.lang.String"/>
            <form-property name="roomname" type="java.lang.String"/>
            <form-property name="custId" type="java.lang.String"/>
            <form-property name="stuempNo" type="java.lang.String"/>
            <form-property name="custName" type="java.lang.String"/>
            <form-property name="useDate" type="java.lang.String"/>
            <form-property name="useWeek" type="java.lang.String"/>
            <form-property name="beginWeek" type="java.lang.String"/>
            <form-property name="endWeek" type="java.lang.String"/>
            <form-property name="week" type="java.lang.String"/>
            <form-property name="beginDate" type="java.lang.String"/>
            <form-property name="endDate" type="java.lang.String"/>
            <form-property name="beginClassNum" type="java.lang.String"/>
            <form-property name="endClassNum" type="java.lang.String"/>
            <form-property name="bindclasses" type="java.lang.String"/>
            <form-property name="courseId" type="java.lang.String"/>
            <form-property name="classno" type="java.lang.String"/>
            <form-property name="departmentName" type="java.lang.String"/>
            <form-property name="ischeck" type="java.lang.Integer"/>
            <form-property name="isopen" type="java.lang.Integer"/>
            <form-property name="faculty" type="java.lang.String"/>
            <form-property name="pid" type="java.lang.String"/>
        </form-bean>

        <form-bean name="reportForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="schoolarea" type="java.lang.String"/>
            <form-property name="build" type="java.lang.String"/>
            <form-property name="roomname" type="java.lang.String"/>
            <form-property name="gardno" type="java.lang.String"/>
            <form-property name="classno" type="java.lang.String"/>
            <form-property name="courseId" type="java.lang.String"/>
            <form-property name="beginDate" type="java.lang.String"/>
            <form-property name="endDate" type="java.lang.String"/>
            <form-property name="termId" type="java.lang.String"/>
            <form-property name="stuempNo" type="java.lang.String"/>
            <form-property name="custName" type="java.lang.String"/>
            <form-property name="departmentName" type="java.lang.String"/>
            <form-property name="deviceid" type="java.lang.String"/>
            <form-property name="type" type="java.lang.String"/>
            <form-property name="termBeginDate" type="java.lang.String"/>
            <form-property name="termEndDate" type="java.lang.String"/>
            <form-property name="accountId" type="java.lang.String"/>
            <form-property name="faculty" type="java.lang.String"/>
            <form-property name="custid" type="java.lang.Integer"/>
            <form-property name="pid" type="java.lang.String"/>
        </form-bean>
        <form-bean name="teacherClassImpForm" type="org.king.upload.web.form.UpLoadForm"/>

        <form-bean name="teacherClassForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="teacherClass" type="org.king.check.domain.TTeacherClass"/>
            <form-property name="areaCode" type="java.lang.String"/>
            <form-property name="faculty" type="java.lang.String"/>
            <form-property name="deptId" type="java.lang.String"/>
            <form-property name="stuempNo" type="java.lang.String"/>
            <form-property name="custName" type="java.lang.String"/>
            <form-property name="departmentName" type="java.lang.String"/>
            <form-property name="shortName" type="java.lang.String" />
            <form-property name="pid" type="java.lang.String" />
        </form-bean>

        <form-bean name="historyForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="areaCode" type="java.lang.String"/>
            <form-property name="faculty" type="java.lang.String"/>
            <form-property name="createYear" type="java.lang.String"/>
            <form-property name="pid" type="java.lang.String"/>
        </form-bean>

        <form-bean name="syncForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="termName" type="java.lang.String"/>
            <form-property name="useWeek" type="java.lang.String"/>
            <form-property name="beginDate" type="java.lang.String"/>
            <form-property name="endDate" type="java.lang.String"/>
        </form-bean>

    </form-beans>


    <action-mappings>
        <action path="/term" name="termForm" scope="request" type="org.king.check.student.web.action.TermAction" parameter="method" validate="false">
            <forward name="termSearch" path="/WEB-INF/pages/stucheck/termSearch.jsp"/>
            <forward name="termEdit" path="/WEB-INF/pages/stucheck/termEdit.jsp"/>
            <forward name="termAdd" path="/WEB-INF/pages/stucheck/termAdd.jsp"/>
        </action>

        <action path="/classdev" name="classdevForm" scope="request" type="org.king.check.student.web.action.ClassdevAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/classdev/classdevList.jsp"/>
            <forward name="edit" path="/WEB-INF/pages/classdev/classdevEdit.jsp"/>
        </action>

        <action path="/todaystuchecksearch" name="reportForm" scope="request" type="org.king.check.student.web.action.StuTodayCheckSearchAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/todaychecksearch/studentcheckList.jsp"/>
            <forward name="realStudent" path="/WEB-INF/pages/todaychecksearch/todayrealstudent.jsp"/>
            <forward name="nocomestudent" path="/WEB-INF/pages/todaychecksearch/nocomestudent.jsp"/>
        </action>


        <!--课程计划导入  -->
        <action path="/coursePlanImp" name="courseImpForm" scope="request" type="org.king.check.student.web.action.CoursePlanAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/courseImp.jsp"/>
            <forward name="self" path="/WEB-INF/pages/stucheck/courseImp.jsp"/>
            <forward name="faile" path="/index.do" redirect="false"/>
        </action>

        <action path="/getPlanModel" parameter="method" type="org.king.check.student.web.action.CoursePlanAction">
        </action>


        <!-- 课程计划查询 -->
        <action path="/coursePlan" name="CourseplanForm" scope="request" type="org.king.check.student.web.action.CoursePlanAction" parameter="method" validate="false">
            <forward name="courseplansearch" path="/WEB-INF/pages/stucheck/searchCourseplan.jsp"></forward>
            <forward name="courseplanlist" path="/WEB-INF/pages/stucheck/searchCourseplan.jsp"></forward>
            <forward name="courseplanedit" path="/WEB-INF/pages/stucheck/courseplanEdit.jsp"></forward>
            <forward name="courseplandelete" path="/coursePlan.do?method=courseplanSearchInit" redirect="true"></forward>
            <forward name="courseplanUpdate" path="/coursePlan.do?method=courseplanSearch"/>
            <forward name="courseplanquery" path="/WEB-INF/pages/stucheck/queryCourseplan.jsp"></forward>
        </action>


        <!--教室导入  -->
        <action path="/roomImp" name="roomImpForm" scope="request" type="org.king.check.student.web.action.RoomAction" parameter="method" validate="false" >
            <forward name="roomImp" path="/WEB-INF/pages/stucheck/roomImp.jsp"/>
            <forward name="importsucc" path="/WEB-INF/pages/stucheck/roomImp.jsp"/>
            <forward name="importfail" path="/WEB-INF/pages/stucheck/roomImp.jsp"/>
        </action>
        <action path="/getRoomModel" type="org.king.check.student.web.action.RoomAction" parameter="method" />


        <!-- 上课时段设置 -->
        <action path="/classTimes" name="classTimesForm" scope="request" type="org.king.check.student.web.action.ClassTimesAction" parameter="method" validate="false" >
            <forward name="list" path="/WEB-INF/pages/stucheck/classTimes.jsp"/>
            <forward name="add" path="/WEB-INF/pages/stucheck/classTimesAdd.jsp"/>
            <forward name="edit" path="/WEB-INF/pages/stucheck/classTimesEdit.jsp"/>
        </action>

        <!--学生考勤统计报表  -->
        <action path="/stucheckreport" name="reportForm" scope="request" type="org.king.check.student.web.action.StuCheckReportAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/stucheckreport.jsp"></forward>
        </action>

        <action path="/classcheckreport" name="reportForm" scope="request" type="org.king.check.student.web.action.ClassCheckReportAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/classcheckreport.jsp"/>
        </action>

        <!--学生学期考勤统计报表  -->
        <action path="/studenttermcheckreport" name="reportForm" scope="request" type="org.king.check.student.web.action.StudentTermCheckReportAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/studenttermcheckreport.jsp"/>
            <forward name="should" path="/WEB-INF/pages/stucheck/termStudentShould.jsp"/>
            <forward name="other" path="/WEB-INF/pages/stucheck/termStudentOther.jsp"/>
        </action>

        <!-- 教师考勤统计报表 -->
        <action path="/teacheckreport" name="reportForm" scope="request" type="org.king.check.student.web.action.TeaCheckReportAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/teacheckreport.jsp"></forward>
        </action>

        <!--教师学期考勤统计报表  -->
        <action path="/teachertermcheckreport" name="reportForm" scope="request" type="org.king.check.student.web.action.TeacherTermCheckReportAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/teachertermcheckreport.jsp"/>
            <forward name="should" path="/WEB-INF/pages/stucheck/termTeacherShould.jsp"/>
            <forward name="other" path="/WEB-INF/pages/stucheck/termTeacherOther.jsp"/>
        </action>

        <!--班级学期考勤统计报表  -->
        <action path="/classtermcheckreport" name="reportForm" scope="request" type="org.king.check.student.web.action.ClassTermCheckReportAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/classtermcheckreport.jsp"/>
            <forward name="should" path="/WEB-INF/pages/stucheck/termClassShould.jsp"/>
            <forward name="other" path="/WEB-INF/pages/stucheck/termClassOther.jsp"/>
            <forward name="planned" path="/WEB-INF/pages/stucheck/plannedClassCheck.jsp"></forward>
            <forward name="valid" path="/WEB-INF/pages/stucheck/validClassCheck.jsp"></forward>
            <forward name="invalid" path="/WEB-INF/pages/stucheck/invalidClassCheck.jsp"></forward>
        </action>

        <!-- 当日上课教室用电查询 -->
        <action path="/roomElec" name="reportForm" scope="request" type="org.king.check.student.web.action.RoomElecAction" parameter="method" validate="false">
            <forward name="roomElec" path="/WEB-INF/pages/stucheck/searchRoomState.jsp"></forward>
        </action>

        <!--当日上课教室用电处理  -->
        <action path="/roomTrans" name="reportForm" scope="request" type="org.king.check.student.web.action.RoomTransAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/roomTrans.jsp"/>
        </action>

        <!-- 处理日志查询 -->
        <action path="/transperson" name="reportForm" scope="request" type="org.king.check.student.web.action.RoomTransAction" parameter="method" validate="false">
            <forward name="transperson" path="/WEB-INF/pages/stucheck/transperson.jsp"/>
        </action>

        <!--取得应到人员列表  -->
        <action path="/allclassstudent" type="org.king.check.student.web.action.AllClassStudentAction" parameter="method">
            <forward name="allClassStudent" path="/WEB-INF/pages/stucheck/allclassstudent.jsp"/>
        </action>

        <!-- 学生日报明细 -->
        <action path="/dailystudentdetail" type="org.king.check.student.web.action.StuDailyDetailAction" parameter="method">
            <forward name="studentdailydetail" path="/WEB-INF/pages/stucheck/dailystudentother.jsp"/>
            <forward name="studentshoud" path="/WEB-INF/pages/stucheck/studentshoud.jsp"/>
        </action>

        <!-- 教师报明细 -->
        <action path="/dailyteacherdetail" type="org.king.check.student.web.action.TeaDailyDetailAction" parameter="method">
            <forward name="teacherdailydetail" path="/WEB-INF/pages/stucheck/dailyteacherother.jsp"/>
            <forward name="teachershoud" path="/WEB-INF/pages/stucheck/dailyteacher.jsp"/>
        </action>

        <!-- 班级报明细 -->
        <action path="/dailyclassstudentdetail" type="org.king.check.student.web.action.ClassDailyDetailAction" parameter="method">
            <forward name="classdailydetail" path="/WEB-INF/pages/stucheck/dailyclassother.jsp"></forward>
            <forward name="classshoud" path="/WEB-INF/pages/stucheck/classshoud.jsp"></forward>
            <forward name="nocome" path="/WEB-INF/pages/stucheck/noComeStudentClass.jsp"></forward>
            <forward name="planned" path="/WEB-INF/pages/stucheck/plannedCheck.jsp"></forward>
            <forward name="valid" path="/WEB-INF/pages/stucheck/validCheck.jsp"></forward>
            <forward name="invalid" path="/WEB-INF/pages/stucheck/invalidCheck.jsp"></forward>
        </action>

        <action path="/deviceCheck" name="reportForm" scope="request" type="org.king.check.student.web.action.DeviceCheckAction" parameter="method" validate="false">
            <forward name="list" path="/WEB-INF/pages/stucheck/deviceCheckList.jsp"/>
            <forward name="detail" path="/WEB-INF/pages/stucheck/viewCheckDetail.jsp"/>
        </action>
        <!-- 手动下发门禁名单 -->
        <action path="/doorList" type="org.king.check.student.web.action.DoorListAction" parameter="method">
            <forward name="result" path="/WEB-INF/pages/stucheck/doorList.jsp"/>
        </action>
        <!-- 手动刷新考勤统计 -->
        <action path="/refreshCheck" type="org.king.check.student.web.action.RefreshCheckAction" parameter="method">
            <forward name="result" path="/WEB-INF/pages/stucheck/refreshCheck.jsp"/>
        </action>
        <!-- 导入辅导员与班级关系 -->
        <action path="/teacherClass" type="org.king.check.student.web.action.TeacherClassAction" parameter="method"
                name="teacherClassImpForm" scope="request">
            <forward name="impl" path="/WEB-INF/pages/stucheck/teacherClassImpl.jsp"/>
            <forward name="faile" path="/index.do" redirect="false"/>
            <forward name="search" path="/WEB-INF/pages/stucheck/teacherClassSearch.jsp"/>
        </action>
        <action path="/teacherClassSearch" type="org.king.check.student.web.action.TeacherClassAction"
                parameter="method" name="teacherClassForm" scope="request">
            <forward name="search" path="/WEB-INF/pages/stucheck/teacherClassSearch.jsp"/>
            <forward name="success" path="/teacherClassSearch.do?method=teacherClassSearch"/>
            <forward name="add" path="/WEB-INF/pages/stucheck/teacherClassAdd.jsp"/>
        </action>
        <!-- 历史数据维护 -->
        <action path="/historyCheck" type="org.king.check.student.web.action.HistoryCheckAction" parameter="method" name="historyForm" scope="request">
            <forward name="result" path="/WEB-INF/pages/stucheck/historyCheck.jsp"/>
        </action>
        <action path="/refreshOperLimit" type="org.king.security.web.action.SecurityAction" parameter="method">
		</action>
        <!-- 课程计划同步 -->
        <action path="/syncCourseplan" type="org.king.check.student.web.action.SyncAction" parameter="method" name="syncForm" scope="request">
		    <forward name="list" path="/WEB-INF/pages/stucheck/courseplanTempList.jsp"/>
            <forward name="syncLog" path="/WEB-INF/pages/stucheck/syncLog.jsp"/>
        </action>
    </action-mappings>
</struts-config>
