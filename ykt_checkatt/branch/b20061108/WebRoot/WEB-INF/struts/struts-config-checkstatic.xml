<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
  
<action-mappings>    
    <!-- 考勤信息统计-->
    <action path="/workStatic" scope="request" type="org.king.check.web.action.WorkStaticAction" parameter="method" validate="false">
    <forward name="goclerkstatic" path="/WEB-INF/pages/check/clerkStatic.jsp" />
    <forward name="clerkstatic" path="/WEB-INF/pages/check/clerkStatic.jsp" />
    <forward name="clerkstaticdetail" path="/WEB-INF/pages/check/clerkStaticDetail.jsp" />
    
    <forward name="godeptstatic" path="/WEB-INF/pages/check/deptStatic.jsp" />
    <forward name="deptstatic" path="/WEB-INF/pages/check/deptStatic.jsp" />
    
    <forward name="gochecklog" path="/WEB-INF/pages/check/checkLogQuery.jsp" />
    <forward name="checklog" path="/WEB-INF/pages/check/checkLogQuery.jsp" />
    </action>
</action-mappings>

</struts-config>
