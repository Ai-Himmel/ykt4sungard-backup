<!DOCTYPE xwork PUBLIC
        "-//Apache Struts//XWork 2.0//EN"
        "http://struts.apache.org/dtds/xwork-2.0.dtd">

<xwork>
	<include file="xwork-default.xml" />
	<bean type="com.opensymphony.xwork2.ObjectFactory" name="smartcard"
		class="com.opensymphony.xwork2.spring.SpringObjectFactory" />
	<constant name="applicationContextPath" value="conf/applicationContext.xml" />

	<package name="smartcard-default" extends="xwork-default"
		abstract="true">

		<result-types>
			<result-type name="void"
				class="com.sungard.smartcard.bccsvr.framework.VoidResult" />
			<result-type name="string"
				class="com.sungard.smartcard.bccsvr.framework.StringResult" />
		</result-types>
	</package>
</xwork>
