<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
	<form-beans>
	    <form-bean name="testCatForm" type="org.apache.struts.validator.DynaValidatorForm">
	        <form-property name="search_testcatname" type="java.lang.String" />
			<form-property name="bmTestCat" type="org.king.business.domain.BmTestCat" />
		</form-bean>
		<form-bean name="testForm" type="org.apache.struts.validator.DynaValidatorForm">
	        <form-property name="search_testcatname" type="java.lang.String" />
			<form-property name="bmTest" type="org.king.business.domain.BmTest" />
		</form-bean>
		<form-bean name="studentForm" type="org.apache.struts.validator.DynaValidatorForm">
	        <form-property name="search_studentno" type="java.lang.String" />
	        <form-property name="search_stuname" type="java.lang.String" />
	        <form-property name="search_deptname" type="java.lang.String" />
	        <form-property name="search_majorname" type="java.lang.String" />
	        <form-property name="search_curgrade" type="java.lang.String" />
	        <form-property name="search_curclass" type="java.lang.String" />
	        <form-property name="search_idtype" type="java.lang.String" />
	        <form-property name="search_idcard" type="java.lang.String" />
	        <form-property name="search_testcatname" type="java.lang.String" />
	        <form-property name="search_testid" type="java.lang.String" />
	        <form-property name="search_ifbm" type="java.lang.String" />
	        <form-property name="search_ifpay" type="java.lang.String" />
			<form-property name="bmStudent" type="org.king.business.domain.BmStudent" />
		</form-bean>
		<form-bean name="testPublishForm" type="org.apache.struts.validator.DynaValidatorForm">
	        <form-property name="search_title" type="java.lang.String" />
			<form-property name="bmTestPublish" type="org.king.business.domain.BmTestPublish" />
		</form-bean>
		
		<form-bean name="feeuploadForm" type="org.king.upload.web.form.UpLoadForm" />
		
		<form-bean name="feeTestForm" type="org.apache.struts.validator.DynaValidatorForm">
		   <form-property name="studentno" type="java.lang.String" />
		   <form-property name="stuname"   type="java.lang.String" />
		   <form-property name="oldtestId"   type="java.lang.String" />
		   <form-property name="newtestId"   type="java.lang.String" />	
		</form-bean>
		
	</form-beans>
	
	<action-mappings>
		<action path="/testCategoryAction" parameter="method" type="org.king.business.web.action.TestCategoryAction" name="testCatForm" scope="request">
			
	    	<forward name="success" path="testcatlist" redirect="false" />
	    	<forward name="testcat" path="testcat" redirect="false" />
	    	<forward name="testcatedit" path="testcatedit" redirect="false" />
	    	<forward name="testcatview" path="testcatview" redirect="false" />
	    	<forward name="testcatlist" path="/testCategoryAction.do?method=search4List" redirect="false" />	    	
	    	    	
			<forward name="faile" path="timeout" redirect="false" />			
	   </action>	
	   
	   <action path="/testAction" parameter="method" type="org.king.business.web.action.TestAction" name="testForm" scope="request">
			
	    	<forward name="success" path="testlist" redirect="false" />
	    	<forward name="test" path="test" redirect="false" />
	    	<forward name="testedit" path="testedit" redirect="false" />
	    	<forward name="testview" path="testview" redirect="false" />
	    	<forward name="testlist" path="/testAction.do?method=search4List" redirect="false" />	    	
	    	    	
			<forward name="faile" path="timeout" redirect="false" />			
	   </action>	
	   	   		
	   <action path="/stuuploadAction" parameter="method" name="feeuploadForm" scope="request" validate="false" unknown="false" type="org.king.business.web.action.StudentAction">
		    <forward name="selfile" path="selfile" />
	        <forward name="importsuccess" path="importsuccess" />
	        <forward name="importfail" path="importfail" />
	        <forward name="gradesselfile" path="gradesselfile" />
	        <forward name="gradesimportsuccess" path="gradesimportsuccess" />
	        <forward name="gradesimportfail" path="gradesimportfail" />
	        <forward name="faile" path="timeout" redirect="false" />	        
		</action>
		
		<action path="/studentAction" parameter="method" name="studentForm" scope="request" validate="false" unknown="false" type="org.king.business.web.action.StudentAction">
		    <forward name="success" path="stutestlist" redirect="false" />
	    	<forward name="stutest" path="stutest" redirect="false" />
	    	<forward name="stutestadd" path="stutestadd" redirect="false" />
	    	<forward name="stutestedit" path="stutestedit" redirect="false" />
	    	<forward name="stutesteditbm" path="stutesteditbm" redirect="false" />
	    	<forward name="stutesteditnone" path="stutesteditnone" redirect="false" />
	    	<forward name="stutestlist" path="/studentAction.do?method=search4List" redirect="false" />
	    	<forward name="stutesttongji" path="stutesttongji" redirect="false" />
	    	<forward name="studentedit" path="studentedit" redirect="false" />
	    	<forward name="historysuccess" path="historylist" redirect="false" />	    	
	    	<forward name="historylistredirect" path="/studentAction.do?method=search4historyList&amp;msg=d0" redirect="true" />
	    	<forward name="historylist" path="/studentAction.do?method=search4historyList" redirect="false" />
	    	
	        <forward name="faile" path="timeout" redirect="false" />	        
		</action>
		
		
		<action path="/feeTestUpdateAction" parameter="method" name="feeTestForm" scope="request" validate="false" unknown="false" type="org.king.business.web.action.FeeTestAction">
		    <forward name="feeTestUpdateQuery" path="feeTestUpdateQuery" redirect="false"/>
		    <forward name="success" path="feeTestUpdateQuery" redirect="false"/>
		    <forward name="faile" path="timeout" redirect="false" />	
		</action>
		
		
		<action path="/testPublishAction" parameter="method" type="org.king.business.web.action.TestPublishAction" name="testPublishForm" scope="request">
			
	    	<forward name="success" path="testpublishlist" redirect="false" />
	    	<forward name="testpublish" path="testpublish" redirect="false" />
	    	<forward name="testpublishedit" path="testpublishedit" redirect="false" />
	    	<forward name="testpublishview" path="testpublishview" redirect="false" />
	    	<forward name="successview" path="testpublishlistview" redirect="false" />
	    	<forward name="testpublishstuview" path="testpublishstuview" redirect="false" />
	    	<forward name="testpublishlist" path="/testPublishAction.do?method=search4List" redirect="false" />	 
	    	
	    	<forward name="loginedit" path="loginedit" redirect="false" />
			<forward name="logineditsuccess" path="/testPublishAction.do?method=load4edit" redirect="false" />	       	
	    	    	
			<forward name="faile" path="timeout" redirect="false" />			
	   </action>
	   <action path="/editor" forward="/editor.jsp" />
	   <action path="/editorview" forward="/editorview.jsp" />
	   
	   <action path="/picUploadAction" parameter="method" type="org.king.business.web.action.PicUploadAction" name="feeuploadForm" scope="request" >
	        <forward name="picUploadsuccess" path="/pages/editor/picupload.jsp" />
	        <forward name="picUploadnoFile" path="picUploadnoFile" redirect="false" />
	        <forward name="picUploadfail" path="picUploadfail" redirect="false" />
	        <forward name="faile" path="timeout" redirect="false" />
	   </action>
	   
	   <action path="/testPublicAction" parameter="method" type="org.king.business.web.action.TestPublicAction" name="testPublishForm" scope="request" validate="false" unknown="false">
	    	<forward name="noticepublist" path="/noticepubAction.do?method=list" redirect="false" />
	     	<forward name="pubsuccess" path="pubsuccess" redirect="false" />
	     	<forward name="pubview" path="pubview" redirect="false" />	    	
	    	<forward name="faile" path="timeout" redirect="false" />
	   </action>
	   	   		
	</action-mappings>
</struts-config>