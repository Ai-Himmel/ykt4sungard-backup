<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
        "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
    <form-beans>
        <form-bean name="monitorRuntimevariablesForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="monitorRuntimevariables" type="org.king.support.domain.MonitorRuntimevariables" />
            <form-property name="type" type="java.lang.String"/>
        </form-bean>
        <form-bean name="monitorContactForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="monitorContact" type="org.king.support.domain.MonitorContact" />
            <form-property name="type" type="java.lang.String"/>
        </form-bean>
        <form-bean name="monitorHostForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="monitorHost" type="org.king.host.domain.MonitorHost" />
            <form-property name="type" type="java.lang.String"/>
        </form-bean>
        <form-bean name="monitorServiceForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="monitorService" type="org.king.hostservice.domain.MonitorService" />
            <form-property name="type" type="java.lang.String"/>
        </form-bean>
        <form-bean name="monitorTermdevServiceForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="monitorTermdevService" type="org.king.termdev.domain.MonitorTermdevService" />
            <form-property name="type" type="java.lang.String"/>
        </form-bean>
        <form-bean name="monitorContactgroupsForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="monitorContactgroups" type="org.king.support.domain.MonitorContactgroups" />
            <form-property name="type" type="java.lang.String"/>
        </form-bean>
        <form-bean name="monitorHostgroupsForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="monitorHostgroups" type="org.king.host.domain.MonitorHostgroups" />
            <form-property name="type" type="java.lang.String"/>
        </form-bean>
        <form-bean name="monitorDevtemForm" type="org.apache.struts.validator.DynaValidatorForm">
            <form-property name="fdeviceName" type="java.lang.String"/>
            <form-property name="deviceName" type="java.lang.String"/>
            <form-property name="templateId" type="java.lang.String"/>
            <form-property name="devtypecode" type="java.lang.String"/>
            <form-property name="monitorDevtem" type="org.king.termdev.domain.MonitorDevtem" />
        </form-bean>
    </form-beans>

    <action-mappings>
        <!-- 运行变量管理 -->
        <action path="/runtimevariables" type="org.king.support.web.action.RuntimevariablesAction"
                name="monitorRuntimevariablesForm" parameter="method" validate="false" scope="request">
            <forward name="input" path="/WEB-INF/pages/support/inputRuntimevariables.jsp" redirect="false"/>
            <forward name="list" path="/WEB-INF/pages/support/listRuntimevariables.jsp" redirect="false"/>
            <forward name="view" path="/WEB-INF/pages/support/viewRuntimevariables.jsp" redirect="false"/>
            <forward name="success" path="/runtimevariables.do?method=listMonitorRuntimevariables" redirect="true"/>
        </action>
        <!-- 联系人管理 -->
        <action path="/contact" type="org.king.support.web.action.ContactAction"
                name="monitorContactForm" parameter="method" validate="false" scope="request">
            <forward name="input" path="/WEB-INF/pages/support/inputContact.jsp" redirect="false"/>
            <forward name="list" path="/WEB-INF/pages/support/listContact.jsp" redirect="false"/>
            <forward name="view" path="/WEB-INF/pages/support/viewContact.jsp" redirect="false"/>
            <forward name="success" path="/contact.do?method=listMonitorContact" redirect="true"/>
            <forward name="selectContactTemplate" path="/WEB-INF/pages/support/selectContactTemplate.jsp" redirect="false"/>
        </action>
        <action path="/addContact" type="org.king.support.web.action.ContactAction"
                name="monitorContactForm" parameter="method" validate="true" scope="request" input="/WEB-INF/pages/support/inputContact.jsp">
            <forward name="success" path="/contact.do?method=listMonitorContact" redirect="true"/>
        </action>
        <action path="/updateContact" type="org.king.support.web.action.ContactAction"
                name="monitorContactForm" parameter="method" validate="true" scope="request" input="/WEB-INF/pages/support/inputContact.jsp">
            <forward name="success" path="/contact.do?method=listMonitorContact" redirect="true"/>
        </action>
        <!-- 主机管理 -->
        <action path="/host" type="org.king.host.web.action.HostAction"
                name="monitorHostForm" parameter="method" validate="false" scope="request">
            <forward name="input" path="/WEB-INF/pages/support/inputHost.jsp" redirect="false"/>
            <forward name="list" path="/WEB-INF/pages/support/listHost.jsp" redirect="false"/>
            <forward name="view" path="/WEB-INF/pages/support/viewHost.jsp" redirect="false"/>
            <forward name="success" path="/host.do?method=listMonitorHost" redirect="true"/>
            <forward name="selectHostTemplate" path="/WEB-INF/pages/support/selectHostTemplate.jsp" redirect="false"/>
        </action>
        <action path="/addHost" type="org.king.host.web.action.HostAction"
                name="monitorHostForm" parameter="method" validate="true" scope="request" input="/WEB-INF/pages/support/inputHost.jsp">
            <forward name="success" path="/host.do?method=listMonitorHost" redirect="true"/>
        </action>
        <action path="/updateHost" type="org.king.host.web.action.HostAction"
                name="monitorHostForm" parameter="method" validate="true" scope="request" input="/WEB-INF/pages/support/inputHost.jsp">
            <forward name="success" path="/host.do?method=listMonitorHost" redirect="true"/>
        </action>
        <!-- 主机服务管理 -->
        <action path="/hostservice" type="org.king.hostservice.web.action.HostserviceAction"
                name="monitorServiceForm" parameter="method" validate="false" scope="request">
            <forward name="input" path="/WEB-INF/pages/support/inputHostservice.jsp" redirect="false"/>
            <forward name="list" path="/WEB-INF/pages/support/listHostservice.jsp" redirect="false"/>
            <forward name="view" path="/WEB-INF/pages/support/viewHostservice.jsp" redirect="false"/>
            <forward name="success" path="/hostservice.do?method=listMonitorService" redirect="true"/>
            <forward name="selectServiceTemplate" path="/WEB-INF/pages/support/selectServiceTemplate.jsp" redirect="false"/>
        </action>
        <action path="/addHostservice" type="org.king.hostservice.web.action.HostserviceAction"
                name="monitorServiceForm" parameter="method" validate="true" scope="request" input="/WEB-INF/pages/support/inputHostservice.jsp">
            <forward name="success" path="/hostservice.do?method=listMonitorService" redirect="true"/>
        </action>
        <action path="/updateHostservice" type="org.king.hostservice.web.action.HostserviceAction"
                name="monitorServiceForm" parameter="method" validate="true" scope="request" input="/WEB-INF/pages/support/inputHostservice.jsp">
            <forward name="success" path="/hostservice.do?method=listMonitorService" redirect="true"/>
        </action>
        <!-- 设备服务管理 -->
        <action path="/termdevService" type="org.king.termdev.web.action.TermdevServiceAction"
                name="monitorTermdevServiceForm" parameter="method" validate="false" scope="request">
            <forward name="input" path="/WEB-INF/pages/support/inputTermdevService.jsp" redirect="false"/>
            <forward name="list" path="/WEB-INF/pages/support/listTermdevService.jsp" redirect="false"/>
            <forward name="view" path="/WEB-INF/pages/support/viewTermdevService.jsp" redirect="false"/>
            <forward name="success" path="/termdevService.do?method=listMonitorTermdevService" redirect="true"/>
        </action>
        <!-- 设备列表 -->
        <action path="/tdevice" type="org.king.termdev.web.action.TDeviceAction"
                name="monitorDevtemForm" parameter="method" validate="false" scope="request">
            <forward name="list" path="/WEB-INF/pages/support/listTDevice.jsp" redirect="false"/>
            <forward name="input" path="/WEB-INF/pages/support/inputDevtem.jsp" redirect="false"/>
        </action>
        <!-- 联系人组管理 -->
        <action path="/contactgroups" type="org.king.support.web.action.ContactgroupsAction"
                name="monitorContactgroupsForm" parameter="method" validate="false" scope="request">
            <forward name="input" path="/WEB-INF/pages/support/inputContactgroups.jsp" redirect="false"/>
            <forward name="list" path="/WEB-INF/pages/support/listContactgroups.jsp" redirect="false"/>
            <forward name="success" path="/contactgroups.do?method=listMonitorContactgroups" redirect="true"/>
        </action>
        <!-- 主机组管理 -->
        <action path="/hostgroups" type="org.king.host.web.action.HostgroupsAction"
                name="monitorHostgroupsForm" parameter="method" validate="false" scope="request">
            <forward name="input" path="/WEB-INF/pages/support/inputHostgroups.jsp" redirect="false"/>
            <forward name="list" path="/WEB-INF/pages/support/listHostgroups.jsp" redirect="false"/>
            <forward name="success" path="/hostgroups.do?method=listMonitorHostgroups" redirect="true"/>
        </action>
    </action-mappings>
</struts-config>
