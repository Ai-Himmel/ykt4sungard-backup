create table t_opersite
(
  opercode varchar(8),
  siteno integer,
  lastsaved varchar(30)
)
begin
action modename ykt_cur end
action add end
action read_by opercode siteno end
action update_by opercode siteno end
action del_by opercode siteno end
action read_lock_by c0 opercode siteno end
action update_lock_by c0 end
action free_lock_by c0 end
end
