#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "cpack.h"
#include "errdef.h"
#include "pubdef.h"
#include "pubdb.h"
#include "pubfunc.h"
#include "dbfunc.h"
#include "fdsqc.h"

EXEC SQL INCLUDE SQLCA;

int F842501(TRUSERID *handle,int iRequest,ST_PACK *rPack,int *pRetCode,char *szMsg)
{
	EXEC SQL BEGIN DECLARE SECTION;
	char    	s_code[10+1]="";              	//专业代码
	char    	s_name[150+1]="";             	//专业名称
	char    	comments[150+1]="";           	//备注
	EXEC SQL END DECLARE SECTION;

	ST_CPACK aPack;
	ST_PACK *out_pack = &(aPack.pack);

	ResetNormalCPack(&aPack,0,1);
#ifdef DEBUG
	writelog(LOG_DEBUG,"s_code         :scust_no       [%s]",rPack->scust_no);
	writelog(LOG_DEBUG,"s_name         :ssectypes      [%s]",rPack->ssectypes);
	writelog(LOG_DEBUG,"comments       :scusttypes     [%s]",rPack->scusttypes);
#endif

	Strncpy_t(s_code,rPack->scust_no,sizeof(s_code));
	Strncpy_t(s_name,rPack->ssectypes,sizeof(s_name));
	Strncpy_t(comments,rPack->scusttypes,sizeof(comments));

	EXEC SQL INSERT INTO ykt_cur.t_cif_speciality (
 s_code,s_name,comments)
	VALUES(:s_code,:s_name,:comments);
	if(SQLCODE)
	{
		db_chk_err(__FILE__,__LINE__,&sqlca);
		*pRetCode=E_DB_SPECIALITY_I;
		goto L_RETU;
	}
	SetCol(handle,0);
	PutRow(handle,out_pack,pRetCode,szMsg);
	return 0;
L_RETU:
	return -1;
}
