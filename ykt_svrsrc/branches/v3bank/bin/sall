#!/bin/bash
currdir=$PWD
cd $HOME/bank/bin
if [ ! -d "$HOME/bank/log" ]; then
  mkdir -p "$HOME/bank/log"
fi
smspid=`ps -fu $LOGNAME|grep "ksmbcc" |grep -v "grep"|grep -v "tail" |awk '{print $2}'`
if [ -z "$smspid" ]; then
  ksmbcc &
else
  echo "ksmbcc has been exist."
fi
smspid=`ps -fu $LOGNAME|grep "start" |grep -v "grep"|grep -v "tail" |awk '{print $2}'`
if [ -z "$smspid" ]; then
  start &
else
  echo "start has been exist."
fi
cd $currdir