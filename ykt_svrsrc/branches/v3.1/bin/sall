#!/bin/bash
cd $HOME/ykt/bin
smspid=`ps aux |grep "drtp" |grep -v "grep"|grep -v "tail" |awk '{print $2}'`
if [ -z $smspid ]; then
  drtp -b -i $HOME/ykt/bin/drtp.ini
else
  echo "drtp has been exist."
fi
bankCheck="checkfile"
if [ ! -d "$bankCheck"]; then
  mkdir "$bankCheck"
fi
smspid=`ps aux |grep "ksmbcc" |grep -v "grep"|grep -v "tail" |awk '{print $2}'`
if [ -z $smspid ]; then
  ksmbcc &
else
  echo "ksmbcc has been exist."
fi
smspid=`ps aux |grep "start" |grep -v "grep"|grep -v "tail" |awk '{print $2}'`
if [ -z $smspid ]; then
  start &
else
  echo "start has been exist."
fi