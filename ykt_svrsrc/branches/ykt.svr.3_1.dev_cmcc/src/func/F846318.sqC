static int doGetSubsidy(T_t_account &tCardAccount,T_t_transdtl &transdtl,double CardMaxBal,int &lastgetsubsidyno)
{
	CAccTrans *pAccTrans=CAccTrans::getInstance();
	TRANS& trans=pAccTrans->trans;
	
	int ret;
	SUBSIDYINFOVECT SubSidyInfoVect;
	lastgetsubsidyno=0;
	if(amtcmp(tCardAccount.balance,CardMaxBal)<0)
	{
		//全部领取		
		trans.totaltransamt=tCardAccount.subsidybal;
		trans.cardaftbal = D4U5(trans.cardaftbal+tCardAccount.subsidybal);
		lastgetsubsidyno=tCardAccount.subsidyno;
	}
	else
	{
		//部分领取
		ret=GetUnGetSubsidyVect(tCardAccount.cardno,SubSidyInfoVect);
		if(ret)
			return ret;
		if(SubSidyInfoVect.size()<1)
		{
			ERRTIP("没有未领取的补助");
			return E_COMMON_ERR;
		}		
		trans.totaltransamt=0;
		double dTransAmt=0.0;
		for(unsigned int i=0;i<SubSidyInfoVect.size();i++)
		{
			dTransAmt = D4U5(dTransAmt+SubSidyInfoVect[i].amount);
			if(amtcmp(trans.cardbefbal+dTransAmt,CardMaxBal)>=0)
			{
				//不止一笔补助,判断是否是负补助
				if(i == SubSidyInfoVect.size()-1)
					break;
				if(amtcmp(SubSidyInfoVect[i+1].amount,0)>0)
					break;
				if(amtcmp(trans.cardbefbal+dTransAmt+SubSidyInfoVect[i+1].amount,CardMaxBal)>=0)
					break;
			}
			if(amtcmp(trans.cardbefbal+dTransAmt,0)<0)
			{
				if(i>0)
					break;
				return ERRIF_CARDBAL_SHORTAGE;
			}
			trans.totaltransamt = D4U5(trans.totaltransamt+SubSidyInfoVect[i].amount);
			lastgetsubsidyno=SubSidyInfoVect[i].subsidyno;
		}
		if(lastgetsubsidyno<1)
		{
			return ERRINFO(E_AMT_EXCEED_MAX,CardMaxBal);
		}
		trans.cardaftbal = D4U5(trans.cardaftbal+trans.totaltransamt);
	}
	if(amtcmp(trans.totaltransamt,0)>0)
	{
		if(amtcmp(trans.cardaftbal,tCardAccount.balance)>0)
		{
			ERRTIP("卡余额异常,请到一卡通管理中心处理");
			return E_COMMON_ERR;
		}
	}
	if(amtcmp(trans.cardaftbal,0)<0)
	{
		return ERRIF_CARDBAL_SHORTAGE;
	}
	//修改补助为已领取状态
	ret=UpdSubsidyStatus(trans,lastgetsubsidyno);
	if(ret)
	{
		return ret;
	}
	
	memset(&transdtl,0,sizeof(transdtl));

	
	tCardAccount.availbal =  D4U5(tCardAccount.availbal+trans.totaltransamt);
	tCardAccount.subsidybal = D4U5(tCardAccount.subsidybal-trans.totaltransamt);
	tCardAccount.lastsubsidyamt=trans.totaltransamt;
	tCardAccount.lastsubsidyno=lastgetsubsidyno;
	tCardAccount.dpscnt = trans.dpscnt;
	tCardAccount.paycnt = trans.paycnt;
	tCardAccount.lastdpscnt=trans.dpscnt;
	tCardAccount.lastpaycnt=trans.paycnt;	
	tCardAccount.lastcardbal=trans.cardbefbal;
	tCardAccount.cardbal=trans.cardaftbal;
	des2src(tCardAccount.lasttransdate,trans.transdate);
	if(amtcmp(trans.cardaftbal,trans.cardbefbal)>0)
	{
		trans.dpscnt++;
		transdtl.transflag=TF_DPS;
		transdtl.cardcnt=trans.dpscnt;
		tCardAccount.dpscnt=trans.dpscnt;
	}
	else
	{
		trans.paycnt++;
		transdtl.transflag=TF_PAY;
		transdtl.cardcnt=trans.paycnt;
		tCardAccount.paycnt=trans.paycnt;
	}
	return 0;
}
static int doGetSubsidyMode1(T_t_account &tCardAccount,T_t_transdtl &transdtl,double CardMaxBal,int &lastgetsubsidyno)
{

	int ret=0;
	CAccTrans *pAccTrans=CAccTrans::getInstance();
	TRANS& trans=pAccTrans->trans;

	SUBSIDYINFOVECT SubSidyInfoVect;
	lastgetsubsidyno=0;
	double dTransAmt=0.0;
	trans.custid=tCardAccount.custid;

	//部分领取
	ret=GetUnGetSubsidyVectByCustId(tCardAccount.custid,SubSidyInfoVect);
	if(ret)
		return ret;
	if(SubSidyInfoVect.size()<1)
	{
		ERRTIP("没有未领取的补助");
		return E_COMMON_ERR;
	}		
	trans.inputamt=0;
	for(unsigned int i=0;i<SubSidyInfoVect.size();i++)
	{
		dTransAmt = D4U5(dTransAmt+SubSidyInfoVect[i].amount);
		if(amtcmp(trans.cardbefbal+dTransAmt,CardMaxBal)>=0)
		{
			//不止一笔补助,判断是否是负补助
			if(i == SubSidyInfoVect.size()-1)
				break;
			if(amtcmp(SubSidyInfoVect[i+1].amount,0)>0)
				break;
			if(amtcmp(trans.cardbefbal+dTransAmt+SubSidyInfoVect[i+1].amount,CardMaxBal)>=0)
				break;
		}
		if(amtcmp(trans.cardbefbal+dTransAmt,0)<0)
		{
			if(i>0)
				break;
			return ERRIF_CARDBAL_SHORTAGE;
		}
		trans.inputamt = D4U5(trans.inputamt+SubSidyInfoVect[i].amount);
		lastgetsubsidyno=SubSidyInfoVect[i].subsidyno;
		writelog(LOG_ERR,"getsubsidy,amt[%.2f]subsidyno[%d]",trans.inputamt,lastgetsubsidyno);
	}
	if(lastgetsubsidyno<1)
	{
		return ERRINFO(E_AMT_EXCEED_MAX,CardMaxBal);
	}

	
	//修改补助为已领取状态
	ret=UpdSubsidyStatusByCustId(trans,lastgetsubsidyno);
	if(ret)
	{
		return ret;
	}
	
	
	strcpy(pAccTrans->trans.cardaccno,tCardAccount.accno);
	pAccTrans->trans.usecardflag=1;	
	pAccTrans->trans.custid=tCardAccount.custid;
	int fundtype=trans.fundtype;
	char voucherno[31]={0};
	des2src(voucherno,trans.voucherno);

	trans.transtype=TRANSTYPE_GETSUBSIDY;
	trans.unusedamt=trans.inputamt;
	trans.transamt = trans.inputamt;
	ret=pAccTrans->DoTransByTransType();
	if(ret)
		return ret;
	if(amtcmp(trans.cardaftbal,0)<0)
	{
		return ERRIF_CARDBAL_SHORTAGE;
	}
	if(amtcmp(trans.cardaftbal,trans.cardbefbal)>0)
	{
		transdtl.transflag=TF_DPS;
		transdtl.cardcnt=trans.dpscnt;
	}
	else
	{
		transdtl.transflag=TF_PAY;
		transdtl.cardcnt=trans.paycnt;
//		tCardAccount.paycnt=trans.paycnt;
	}
	return 0;
}


int F846318(TRUSERID *handle,int iRequest,ST_PACK *rPack,int *pRetCode,char *szMsg)
{
	int ret=0;
	int subsidyMode = 0;
	char temp[128]="";
	int lastgetsubsidyno=0;
	CAccTrans *pAccTrans=CAccTrans::getInstance();


	ret = GetSysParaVal(SYSPARA_SUBSIDYMODE,temp);
	if(ret)
	{
		if(DB_NOTFOUND != ret)
		{
			writelog(LOG_ERR,"GetSysParaVal[%d]ret=[%d]",SYSPARA_SUBSIDYMODE,ret);
			return E_DB_SYSPARA_R;
		}
		subsidyMode = 0;
	}
	else
	{
		subsidyMode = atoi(temp);
		subsidyMode = (subsidyMode > 1) ? 1 : subsidyMode;
	}
	
	pAccTrans->trans.transcode=TC_SUBSIDYGET;
	pAccTrans->trans.termid=rPack->lwithdraw_flag;
	if(!pAccTrans->trans.termid)
		return E_INPUTNULL_TERMID;
	ret=pAccTrans->InitTrans();
	if(ret)
		return ret;			
	TRANS& trans=pAccTrans->trans;
	T_t_transdtl transdtl;
	memset(&transdtl,0,sizeof transdtl);
	
	ST_CPACK aPack;
	ST_PACK *outPack = &(aPack.pack);

	ResetNormalCPack(&aPack,0,1);
		
	
	SetCol(handle,F_LVOL0,F_LVOL8,F_SDATE3,F_SEMP,F_LSERIAL1,F_SSTATION0,F_VSMESS,0);		
	trans.cardflag=1;
	trans.usecardflag=1;	
	trans.cardno=rPack->lvol0;	
	trans.purseno=PURSE_NO_ONE;
	trans.dpscnt=rPack->lvol5;
	trans.paycnt=rPack->lvol6;
	trans.cardbefbal=rPack->lvol7/100.0;
	trans.cardaftbal=trans.cardbefbal;
	trans.cardsubsidyno=rPack->lvol4;
//	des2src(trans.cardphyid,rPack->sstation1);
	T_t_card tCard;
	memset(&tCard,0,sizeof(tCard));
	ret=doCard(trans,tCard);
	if(ret)
	{
		return ret;
	}
	trans.feetype=tCard.feetype;
	trans.cardtype=tCard.cardtype;
	T_t_account tCardAccount;	
	memset(&tCardAccount,0,sizeof(tCardAccount));
	ret=DB_t_account_read_by_accno(tCard.accno,&tCardAccount);
	if(ret)
	{
		if(DB_NOTFOUND==ret)
			return ERRINFO(E_NOTEXIST_CARDACCNO,tCard.accno);
		else 
			return E_DB_ACCOUNT_R;
	}
	if(tCardAccount.status[0]!=STATUS_NORMAL)
	{
		return ERRINFO(E_CARDACC_LOGOUT,tCardAccount.accno);
	}
	double CardMaxBal=0;
	ret=GetPurseMaxBal(PURSE_NO_ONE,CardMaxBal);
	if(ret)
		return ret;

	if(subsidyMode==0)
	{

		///
		ret=DB_t_account_read_lock_by_c0_and_accno(tCard.accno,&tCardAccount);
		if(ret)
		{
			if(DB_NOTFOUND==ret)
				return ERRINFO(E_NOTEXIST_CARDACCNO,tCard.accno);
			else 
				return E_DB_ACCOUNT_R;
		}
		ret = doGetSubsidy(tCardAccount,transdtl,CardMaxBal,lastgetsubsidyno);
		if(ret)
		{
			writelog(LOG_ERR,"doGetSubsidy,ret=[%d]",ret);
			return ret;
		}
		ret=DB_t_account_update_lock_by_c0(&tCardAccount);
		if(ret)
		{
			return E_DB_ACCOUNT_U;
		}
	}
	else
	{
		ret = doGetSubsidyMode1(tCardAccount,transdtl,CardMaxBal,lastgetsubsidyno);
		if(ret)
		{
			writelog(LOG_ERR,"doGetSubsidyMode1,ret=[%d]",ret);
			return ret;
		}
		
	}
	trans.custid=tCard.custid;
	
	des2src(transdtl.transdate,trans.transdate);
	des2src(transdtl.transtime,trans.transtime);
	strcpy(transdtl.accdate,trans.accdate);
	strcpy(transdtl.acctime,trans.acctime);
	transdtl.termid=trans.termid;
	transdtl.termseqno=trans.termseqno;
	transdtl.transcode=trans.transcode;
	transdtl.paytype=trans.fundtype;
//	des2src(transdtl.voucherno,trans.voucherno);
	transdtl.cardno=trans.cardno;
	transdtl.purseno=trans.purseno;
	transdtl.cardbefbal=trans.cardbefbal;
	transdtl.managefee=trans.totalfeeamt;
	transdtl.amount=trans.totaltransamt;
	if(amtcmp(transdtl.amount,0)<0)
		transdtl.amount = -transdtl.amount;
	transdtl.cardaftbal=trans.cardaftbal;
	transdtl.custid=tCardAccount.custid;
	des2src(transdtl.custname,tCardAccount.accname);
	des2src(transdtl.opercode,trans.opercode);
	transdtl.status[0]=DTLSTATUS_SUCCESS;
    des2src(transdtl.showcardno,tCard.showcardno);
	GetStuempnoByCustid(transdtl.custid,transdtl.stuempno);
	ret=DB_t_transdtl_add(&transdtl);
	if(ret)
	{
	  if(DB_REPEAT==ret)
		return E_DB_TRANSDTL_E;
	  else
		return E_DB_TRANSDTL_I;
	}		
	SetCol(handle,F_LVOL0,F_LVOL8,F_LVOL4,F_DAMT0,
		F_SDATE3,F_SEMP,F_LWITHDRAW_FLAG,F_LSERIAL1,F_VSMESS,0);	
	outPack->lvol0=trans.cardno;
	outPack->lvol8=D4U5(trans.cardaftbal*100,0);
	des2src(outPack->sdate3,pAccTrans->trans.accdate);
	outPack->lwithdraw_flag=trans.termid;
	outPack->lserial1=trans.termseqno;
	outPack->lvol4=lastgetsubsidyno;
	outPack->damt0=trans.totaltransamt;
	sprintf(temp,"补助批次号:%d 卡余额:%.2f元",lastgetsubsidyno,trans.cardaftbal);
	pAccTrans->remark+=temp;
	strcpy(outPack->vsmess,pAccTrans->remark.c_str());
	writelog(LOG_INFO,outPack->vsmess);	
	PutRow(handle,outPack,pRetCode,szMsg);
	return 0;
}
