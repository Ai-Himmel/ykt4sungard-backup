create table t_subject
(
  subjno varchar2(20),
  subjname varchar2(60),
  subjtype number,
  balflag number,
  fsubjno varchar2(20),
  subjlevel number,
  endflag char(1)
)
begin
dbaction modename ykt_cur end
dbaction read_by subjno end
dbaction open_select_by c0 end
dbaction fetch_select_by c0 end
dbaction close_select_by c0 end
dbaction open_select_by c1 fsubjno end
dbaction fetch_select_by c1  end
dbaction close_select_by c1 end
end
