CREATE TABLE T_PIF_FIXCARD
(
    CARDNO       		INTEGER       NOT NULL,
    TRADECNT		    INTEGER				NOT NULL,
    OPERATE_DATE    VARCHAR(8)		NOT NULL,
    OPERATE_TIME    VARCHAR(6)		NOT NULL,
    PHYSICALNO    	VARCHAR(8),
    FIX_DATE    		VARCHAR(8),
    FIX_AMT       	DECIMAL(9,2),
    OPER_CODE       VARCHAR(10)
)

begin
action modename YKT_CUR end
action add end
action read_by CARDNO TRADECNT OPERATE_DATE OPERATE_TIME end
end
