CREATE TABLE T_TIF_LIBDEPOSIT
(
    CUSTOMER_ID     INTEGER       NOT NULL,
    CUT_NAME        CHARACTER(60),
    DEPOSIT_FEE     DECIMAL(9,2),
    FEE_FLAG       	CHAR(1),
    OPER_DATE      	VARCHAR(10),
    OPER_TIME      	VARCHAR(8)
)
begin
action modename YKT_CUR end
action add end
action read_by CUSTOMER_ID end
action update_by CUSTOMER_ID end
action read_lock_by cur CUSTOMER_ID end
action update_lock_by cur end
action free_lock_by cur end
end
