create table c_m_applications
(
	cm_date        varchar(10),
	cm_time        varchar(10),
	cm_card_no     varchar(30),
	cm_serices_no  varchar(30),
	cm_count      integer,
	cm_pro        integer,
	cm_type       integer
)
begin
action modename ykt_cur end
action add end
action del_by cm_card_no  end
action read_by cm_card_no cm_pro end
action open_select_by c1 cm_pro end
action fetch_select_by c1 end
action close_select_by c1 end
end
