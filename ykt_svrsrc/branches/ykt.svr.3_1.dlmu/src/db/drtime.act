create table t_doortime
(
  timeid number,
  timename varchar2(60),
  starttime char(6),
  endtime char(6),
  adddelflag number(1),
  downstatus number,
  updatetime varchar(30)
)
begin
action modename ykt_cur end
action add end
action read_by timeid end
action update_by timeid end
action del_by timeid end
action read_lock_by c0 timeid end
action update_lock_by c0 end
action free_lock_by c0 end
end
