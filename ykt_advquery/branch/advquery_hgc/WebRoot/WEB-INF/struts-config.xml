  	<!--
  	<action  path="/updateRegisterComment" name="registerInfoForm" type="com.kingstargroup.advquery.register.action.RegisterInfoAction">
  	   <forward name="register.comments" path="/register/registerComments.jsp"/>
  	</action>
  	-->
  	
  	<!--学生付费 -->
  	<action path="/feeQuery" forward="fee.query"/>
