<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="dormForm"
			type="org.apache.struts.action.DynaActionForm">
			<form-property name="dormitory"
				type="org.king.dormitorymanage.domain.Dormitorymodel" />
		</form-bean>
		<form-bean name="uploadForm"
			type="org.king.upload.web.form.UpLoadForm" />
		<form-bean name="searchForm"
			type="org.apache.struts.action.DynaActionForm">
			<form-property name="area" type="java.lang.String" />
			<form-property name="dorm" type="java.lang.String" />
			<form-property name="unit" type="java.lang.String" />
			<form-property name="dormfee" type="java.lang.String" />
			<form-property name="roomNumber" type="java.lang.String" />
			<form-property name="collegeId" type="java.lang.String" />
			<form-property name="classId" type="java.lang.String" />
			<form-property name="managerId" type="java.lang.String" />
			<form-property name="studentNo" type="java.lang.String" />
			<form-property name="stuName" type="java.lang.String" />
			<form-property name="gender" type="java.lang.String" />
			<form-property name="majorin" type="java.lang.String" />
			<form-property name="nation" type="java.lang.String" />
			<form-property name="enterenceNo" type="java.lang.String" />			
			<form-property name="district" type="java.lang.String" />			
			<form-property name="fromProvince" type="java.lang.String" />
			<form-property name="location" type="java.lang.String" />
		</form-bean>
		<form-bean name="adjustForm"
			type="org.apache.struts.action.DynaActionForm">
			<form-property name="stuNo" type="java.lang.String" />
			<form-property name="stuName" type="java.lang.String" />
			<form-property name="gender" type="java.lang.String" />

			<form-property name="area" type="java.lang.String" />

			<form-property name="location" type="java.lang.String" />
			<form-property name="dormitory" type="java.lang.String" />
			<form-property name="unit" type="java.lang.String" />
			<form-property name="roomNumber" type="java.lang.String" />

			<form-property name="areaA" type="java.lang.String" />
			<form-property name="areaB" type="java.lang.String" />
			<form-property name="unitA" type="java.lang.String" />
			<form-property name="unitB" type="java.lang.String" />
			<form-property name="locationA" type="java.lang.String" />
			<form-property name="dormitoryA" type="java.lang.String" />
			<form-property name="roomNumberA" type="java.lang.String" />
			<form-property name="locationB" type="java.lang.String" />
			<form-property name="dormitoryB" type="java.lang.String" />
			<form-property name="roomNumberB" type="java.lang.String" />
			<form-property name="startDate" type="java.lang.String" />
			<form-property name="endDate" type="java.lang.String" />
			<form-property name="bindStudents" type="java.lang.String" />
			<form-property name="unbindStudents"
				type="java.lang.String" />
			<form-property name="bindStudentsA" type="java.lang.String" />
			<form-property name="bindStudentsB" type="java.lang.String" />



		</form-bean>
	</form-beans>
	<action-mappings>
		<action path="/DormitoryAction" name="dormForm" scope="request"
			parameter="method"
			type="org.king.dormitorymanage.web.action.DormitoryAction">
			<forward name="init4Maintain" path="dormitoryinit4Maintain" />
			<forward name="init4Add" path="dormitoryinit4Add" />
			<forward name="saveSuccess" path="dormitorysaveSuccess" />
			<forward name="searchSuccess" path="dormitorysearchSuccess" />
			<forward name="revise" path="dormitoryrevise" />
			<forward name="reviseOver" path="dormitoryreviseOver" />
			<forward name="operationFailure"
				path="dormitoryoperationFailure" />
			<forward name="saveAlert" path="dormitorysaveAlert" />
			<forward name="dormStat" path="dormitorydormStat" />
			<forward name="dormStat_yg" path="dormitorydormStat_yg" />
			<forward name="roomStat" path="dormitoryroomStat" />
			<forward name="notcheckinroomStat" path="notcheckindormitoryroomStat" />
			<forward name="dormStat_hlxy" path="dormitorydormStat_hlxy" />
			<forward name="roomStat_hlxy" path="dormitoryroomStat_hlxy" />
			<forward name="roomStat_yg" path="dormitoryroomStat_yg" />
			<forward name="searchOver" path="dormitorysearchOver" />
			<forward name="dormitorylistgoto"
				path="/DormitoryAction.do?method=searchAll" redirect="true" />

		</action>
		<action path="/DormitorySearchAction" name="searchForm"
			scope="request" parameter="method"
			type="org.king.dormitorymanage.web.action.DormitoryAction">
			<forward name="searchOver" path="dormitorysearchOver" />
			<forward name="init4Search" path="dormitoryinit4Search" />

			<forward name="init4Search_hlxy"
				path="dormitoryinit4Search_hlxy" />
			<forward name="init4Search_yg"
				path="dormitoryinit4Search_yg" />
			<forward name="init4fdySearch"
				path="dormitoryinit4fdySearch" />
			
			<forward name="init4fdySearch_hlxy"
				path="dormitoryinit4fdySearch_hlxy" />
			
			<forward name="init4FeeSearch"
				path="dormitoryinit4FeeSearch" />
			<forward name="singleSearch" path="singleSearch" />
            <forward name="timeup" path="timeup" redirect="false" />
		</action>
		<action path="/DormitoryAdjustAction" name="adjustForm"
			scope="request" parameter="method"
			type="org.king.dormitorymanage.web.action.DormitoryAction">
			<forward name="init4Adjust" path="dormitoryinit4Adjust" />
			<forward name="init4Adjust_yg"
				path="dormitoryinit4Adjust_yg" />
			<forward name="init4Adjust_hlxy"
				path="dormitoryinit4Adjust_hlxy" />

			<forward name="searchAdjust" path="dormitorysearchAdjust" />
			<forward name="searchAdjust_yg"
				path="dormitorysearchAdjust_yg" />
			<forward name="searchHisOver" path="dormitorysearchHisOver" />
			<forward name="init4AdjustHis"
				path="dormitoryinit4AdjustHis" />

			<forward name="init4AdjustHis_hlxy"
				path="dormitoryinit4AdjustHis_hlxy" />
			<forward name="init4AdjustHis_yg"
				path="dormitoryinit4AdjustHis_yg" />
			<forward name="twoDormAdjustList" path="twoDormAdjustList" />

			<forward name="twoDormAdjustList_hlxy"
				path="twoDormAdjustList_hlxy" />
			
			<forward name="twoDormAdjustList_yg"
				path="twoDormAdjustList_yg" />

			<forward name="twoDormfdyAdjustList"
				path="twoDormfdyAdjustList" />
			
			<forward name="twoDormfdyAdjustList_hlxy"
				path="twoDormfdyAdjustList_hlxy" />	
			<forward name="timeup" path="timeup" redirect="false" />		
		</action>

		<action path="/DormitoryuploadAction" parameter="method"
			type="org.king.dormitorymanage.web.action.DormitoryAction"
			name="uploadForm" scope="request" input="input">
			<forward name="init4Upload" path="dormitoryinit4Upload" />
		</action>

		<action path="/getDormitoryXMLMenuTree"
			type="org.king.dormitorymanage.web.action.DormitoryAction"
			name="menuForm" scope="request" input="menulist" parameter="method"
			unknown="false" validate="false">
			<forward name="success"
				path="/WEB-INF/pages/dormitory/XMLMenuTree.jsp" redirect="false" />
		</action>

		<action path="/DormitoryListAction"
			type="org.king.dormitorymanage.web.action.DormitoryAction"
			parameter="method" name="menuForm" validate="false"
			unknown="false">
			<forward name="listAllDorm"
				path="/WEB-INF/pages/dormitory/menuTree.jsp" redirect="false" />
			<forward name="listAllMajorin"
				path="/WEB-INF/pages/dormitory/majorinTree.jsp" redirect="false" />
		</action>

		<action path="/GzDormDistrAction"
			type="org.king.dormitorymanage.web.action.GzDormitoryDistrAction"
			parameter="method" name="adjustForm" validate="false" unknown="false">
			<forward name="dormDistributionStatistic"
				path="gzdormdistr" redirect="false" />
			<forward name="dormDistributionSuccess"
				path="gzdormdistrsuccess"
				redirect="false" />
		</action>
	</action-mappings>
</struts-config>