		</action>

		<action path="/classAction" type="org.king.classmanage.web.action.ClassAction" name="classForm" parameter="method" scope="request" validate="false">
			<forward name="list" path="classList" />
			
			<forward name="list_hlxy" path="classList_hlxy" />
			
			<forward name="add" path="classEdit" />
			<forward name="edit" path="classEdit" />
			
			<forward name="edit_hlxy" path="classEdit_hlxy" />
						
			<forward name="classsearchlist" path="classsearchList" />
			
			<forward name="classsearchlist_hlxy" path="classsearchList_hlxy" />			
			
