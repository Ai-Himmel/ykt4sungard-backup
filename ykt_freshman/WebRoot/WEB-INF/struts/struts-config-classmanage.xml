			<forward name="nationStatistic" path="nationStatistic" />
			<forward name="majorinStatistic" path="majorinStatistic" />
			<forward name="nativeplaceStatistic" path="nativeplaceStatistic" />
			<forward name="nationalityStatistic" path="nationalityStatistic" />
			<forward name="daleiStatistic" path="daleiStatistic" />
			
