<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>		
		<form-bean name="graduateTypeSearchForm" type="org.apache.struts.action.DynaActionForm">
		    <form-property name="graduateTypeSearch" type="java.lang.String"/>
		</form-bean>		
		<form-bean name="graduateTypeForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="id" type="java.lang.String"/>
			<form-property name="graduateType" type="java.lang.String"/>
			<form-property name="updator" type="java.lang.String"/>
			<form-property name="updateTime" type="java.lang.String"/>
		</form-bean>
		<form-bean name="dormAppSearchForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="graduateNoSearch" type="java.lang.String"/>
			<form-property name="sex" type="java.lang.String"/>
			<form-property name="distriction" type="java.lang.String"/>
			<form-property name="residentneed" type="java.lang.String"/>
			<form-property name="graduateNoSearch1" type="java.lang.String"/>
			<form-property name="graduateNoSearch2" type="java.lang.String"/>			
			<form-property name="graduateNameSearch" type="java.lang.String"/>
			<form-property name="studentTypeSearch" type="java.lang.String"/>
			<form-property name="matriculateTypeSearch" type="java.lang.String"/>
			<form-property name="curCollegeSearch" type="java.lang.String"/>
			<form-property name="specialitySearch" type="java.lang.String"/>
			<form-property name="lengthofschoolSearch" type="java.lang.String"/>
			<form-property name="graduateTypeSearch" type="java.lang.String"/>
			<form-property name="memo" type="java.lang.String"/>
		</form-bean>
		<form-bean name="dormAppBillForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="id" type="java.lang.String"/>
			<form-property name="enterenceNo" type="java.lang.String"/>
			<form-property name="studentNo" type="java.lang.String"/>
			<form-property name="district" type="java.lang.String"/>
			<form-property name="curAddress" type="java.lang.String"/>
			<form-property name="curTelephone" type="java.lang.String"/>
			<form-property name="ifNeedDorm" type="java.lang.String"/>
			<form-property name="memo" type="java.lang.String"/>
		</form-bean>
		<form-bean name="deptBundSearchForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="deptcodeSearch" type="java.lang.String"/>
			<form-property name="sexSearch" type="java.lang.String"/>
		</form-bean>
		<form-bean name="buildBundSearchForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="locationSearch" type="java.lang.String"/>
			<form-property name="dormitorySearch" type="java.lang.String"/>
		</form-bean>
	</form-beans>	
	<action-mappings>
		<action path="/graduateTypeAction" name="graduateTypeSearchForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.DormTypeManageAction">
			<forward name="graduateTypeList" path="graduateTypeList" redirect="false"/>		
			<forward name="graduateTypeDeleteSuccess" path="/graduateTypeAction.do?method=loadDormTypeList" redirect="false"/>		
				
		</action>
		<action path="/graduateTypeBillAction" name="graduateTypeForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.DormTypeManageAction">
			<forward name="graduateTypeInfo" path="graduateTypeInfo" redirect="false"/>		
			<forward name="graduateTypeSaveSuccess" path="/graduateTypeAction.do?method=loadDormTypeList" redirect="false"/>		
		</action>
		<action path="/dormAppBillAction" name="dormAppBillForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.DormAppAction">
			<forward name="updateGraduateSuccess" path="/dormAppAction.do?method=loadGraduateList" redirect="false"/>
			<forward name="gzdormappInfo" path="gzdormappInfo_tiles" />
			<forward name="memoView" path="/WEB-INF/pages/yangong/memoView.jsp" />
			<forward name="memoView4hl" path="/WEB-INF/pages/yangong/memoView4hl.jsp" />
			<forward name="gzdormappInfo4s" path="gzdormappInfo4s_tiles" />			
			<forward name="gzdormAppList" path="/dormAppAction.do?method=loadGzList" />					
			<forward name="updateSuccess" path="/dormAppAction.do?method=loadGzList" redirect="false"/>
			<forward name="gzdormappcannomodify" path="/dormAppAction.do?method=loadGzList" redirect="false"/>
			<forward name="haveDormappinfo" path="/dormAppAction.do?method=load4SingleDormApp4Gz" redirect="false"/>
			<forward name="nothaveDormappinfo" path="gzdormappInfo4s_tiles" redirect="false"/>
			<forward name="timeup" path="timeup" redirect="false" />
		</action>
		<action path="/dormAppAction" name="dormAppSearchForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.DormAppAction">
			
			<forward name="dormAppList" path="/WEB-INF/pages/yangong/dormAppList.jsp" redirect="false"/>

			<forward name="dormAppList" path="dormAppList_tiles" />

			<forward name="gzdormAppList" path="gzdormAppList_tiles" />
						
			<forward name="gzdormAppList4sx" path="gzdormAppList4sx_tiles" />
			
			<forward name="gzdormappDeleteSuccess" path="/dormAppAction.do?method=loadGzList" redirect="false"/>
			
			<forward name="dormAppList4graduate" path="/WEB-INF/pages/yangong/dormAppList4graduate.jsp" redirect="false"/>
			
			<forward name="cannotappdorm" path="/WEB-INF/pages/yangong/cannotappdorm.jsp" redirect="false"/>	
									
			<forward name="updateGraduateSuccess" path="/dormAppAction.do?method=loadGraduateList" redirect="false"/>
			
			<forward name="updateGzSuccess" path="/dormAppAction.do?method=loadGzList4Need" redirect="false"/>
			
			<forward name="updateGraduateSuccess4graduate" path="/dormAppAction.do?method=loadGraduateByID" redirect="false"/>
			
			<forward name="haveDormappinfo" path="/WEB-INF/pages/yangong/dormAppsingle4hl.jsp" redirect="false"/>
			
			<forward name="nothaveDormappinfo" path="/dormAppBillAction.do?method=loadGzDormAppInfo4s" redirect="false"/>
			
			<forward name="gotoWelcome" path="/WEB-INF/pages/about.jsp" redirect="false"/>
			
			<forward name="timeup" path="timeup" redirect="false" />
		</action>
		
		<action path="/deptDormBundTJAction" name="deptBundSearchForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.DormAppAction">
			<forward name="deptDormBundTJSucc" path="/WEB-INF/pages/yangong/deptDormBundTJ.jsp" redirect="false"/>
			<forward name="deptDormBundingTJSucc" path="/WEB-INF/pages/yangong/deptDormBundingTJ.jsp" redirect="false"/>				
		</action>
	    <action path="/buildDormBundTJAction" name="buildBundSearchForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.DormAppAction">
			<forward name="buildDormBundTJSucc" path="/WEB-INF/pages/yangong/buildDormBundTJ.jsp" redirect="false"/>
			<forward name="buildDormBundingTJSucc" path="/WEB-INF/pages/yangong/buildDormBundingTJ.jsp" redirect="false"/>				
		</action>
	</action-mappings>	
</struts-config>