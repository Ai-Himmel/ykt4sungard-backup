<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="YxGraduateForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="YxGraduate" type="org.king.yangong.dormitorymanage.domain.YxGraduate" />
			<form-property name="graduateNoSearch1" type="java.lang.String"/>
			<form-property name="graduateNoSearch2" type="java.lang.String"/>
			<form-property name="lengthofschool" type="java.lang.String"/>	
			<form-property name="lengthofschool1" type="java.lang.String"/>							
			 <form-property name="startfrom" type="java.lang.String"/>
			 <form-property name="newGraduateType" type="java.lang.String"/>
			 <form-property name="newlengthofschool" type="java.lang.String"/>
			 <form-property name="newdistriction" type="java.lang.String"/>
			 <form-property name="newrequestavailable" type="java.lang.String"/>	 
		</form-bean>
		<form-bean name="collegeDormBindingForm" type="org.apache.struts.action.DynaActionForm">
			 <form-property name="sex" type="java.lang.String"/>
			 <form-property name="studentType" type="java.lang.String"/>
			 <form-property name="curCollege" type="java.lang.String"/>
			 <form-property name="section" type="java.lang.String"/>
			 <form-property name="distriction" type="java.lang.String"/>	 
			 <form-property name="building" type="java.lang.String"/>
			 <form-property name="layer" type="java.lang.String"/>	
			 <form-property name="cell" type="java.lang.String"/>
			 <form-property name="binddormitorys" type="java.lang.String"/>	
			 <form-property name="unbinddormitorys" type="java.lang.String"/>
		</form-bean>
	</form-beans>	
	<action-mappings>
		<action path="/yxGraduateAction" name="YxGraduateForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.YxGraduateAction">
			<forward name="load4Search" path="GraduateLoad4Search"/>		
			<forward name="load4ResidentSearch" path="GraduateLoad4ResidentSearch"/>		
			<forward name="dormDistributionStatistic" path="GraduatedormDistributionStatistic"/>	
			<forward name="dormDistributionSuccess" path="GraduatedormDistributionSuccess"/>
			<forward name="faile" path="timeout" redirect="false" />
			<forward name="menutimelist" path="ygmenutimelist" />
			<forward name="sxTJ" path="sxTJ" />
			<forward name="updateSucc" path="/yxGraduateAction.do?method=load4List" />
			<forward name="residentUpdateSucc" path="/yxGraduateAction.do?method=load4ResidentList" />
			<forward name="mtsuccess" path="/yxGraduateAction.do?method=menutimelist" redirect="false" />	
		</action>	
		<action path="/collegeDormBindingAction" name="collegeDormBindingForm" scope="request" parameter="method" type="org.king.yangong.dormitorymanage.web.action.CollegeDormBindingAction">
			<forward name="load4Search" path="colldormLoad4Search"/>
			<forward name="dormbindresult" path="dormbindresult"/>		
		</action>	
	</action-mappings>	
</struts-config>