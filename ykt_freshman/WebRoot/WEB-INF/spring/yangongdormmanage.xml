<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="yxGraduateDAO"
		class="org.king.yangong.dormitorymanage.dao.hibernate.YxGraduateDAOHibernate">
		<property name="baseDAO">
			<ref bean="baseDAO" />
		</property>
	</bean>
	<bean id="yxGraduateService"
		class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean">
		<property name="transactionManager">
			<ref bean="transactionManager" />
		</property>
		<property name="target">
			<bean
				class="org.king.yangong.dormitorymanage.service.impl.YxGraduateServiceImpl">
				<property name="yxGraduateDAO">
					<ref local="yxGraduateDAO" />
				</property>
				<property name="dormitorymodelDAO">
					<ref bean="dormitorymodelDAO" />
				</property>
			</bean>
		</property>
		<property name="transactionAttributes">
			<props>
				<prop key="add*">PROPAGATION_REQUIRED</prop>
				<prop key="save*">PROPAGATION_REQUIRED</prop>
				<prop key="find*">PROPAGATION_REQUIRED,readOnly</prop>
				<prop key="*">PROPAGATION_REQUIRED,readOnly</prop>
				<prop key="remove*">PROPAGATION_REQUIRED</prop>
				<prop key="delete*">PROPAGATION_REQUIRED</prop>
				<prop key="update*">PROPAGATION_REQUIRED</prop>
			</props>
		</property>
	</bean>

</beans>
