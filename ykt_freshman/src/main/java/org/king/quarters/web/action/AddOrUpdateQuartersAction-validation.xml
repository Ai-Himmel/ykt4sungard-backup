<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.dtd">
<validators>
    
   
   <field name="model.area">
   		<field-validator type="requiredstring">
   			<message>校区不能为空</message>
   		</field-validator>
   </field>
   <field name="model.location">
   		<field-validator type="requiredstring">
   			<message>区域不能为空</message>
   		</field-validator>
   </field>
   <field name="model.dormitory">
   		<field-validator type="requiredstring">
   			<message>宿舍号不能为空</message>
   		</field-validator>
   </field>
   <field name="model.level">
   		<field-validator type="requiredstring">
   			<message>层号不能为空</message>
   		</field-validator>
   </field>
   <field name="model.roomNumber">
   		<field-validator type="requiredstring">
   			<message>宿舍号不能为空</message>
   		</field-validator>
   </field>
   
   <validator type="expression">
    <param name="expression">
    <![CDATA[(model.accomodation>=model.distributeNum)]]>
    </param>
    <message>可分配床位数不能比总床位数多!</message>
   </validator>
</validators>
