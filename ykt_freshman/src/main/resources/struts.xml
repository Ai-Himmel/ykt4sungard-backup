    	</result-types>
 		<action name="summaryQuarters" class="org.king.summary.web.action.QuartersSummaryAction">
 			<result name="input"  type="jasper">
 			    <param name="location">/WEB-INF/pages/summary/quartersSummary.jasper</param>
 			    <param name="dataSource">dataSource</param>
 			    <param name="format">XLS</param>
 			    <param name="documentName">summary.doc</param>

 			</result>		
 		</action>
		<action name="queryGraduate" class="org.king.summary.web.action.QueryGraduateAction">
 			<result name="input">/WEB-INF/pages/summary/graduateInfoList.jsp</result>		
 		</action>
 		
    </package>
