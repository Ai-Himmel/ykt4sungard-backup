<struts>
  
    <include file="mystruts.xml" />
    
    <package name="/quarters" extends="mydefault">
    	<result-types>
    		<result-type name="jasper" class="org.apache.struts2.plugin.jasperreports.views.JasperReportsResult"/>
    	</result-types>
    	<!-- 宿舍使用部门维护 -->
 		<action name="listDept" class="org.king.quarters.web.action.ListDeptAction">
 			<result name="input">/WEB-INF/pages/quarters/deptList.jsp</result>
 		</action>
 		<action name="addOrUpdateDept" class="org.king.quarters.web.action.AddOrUpdateDeptAction">
 			<result name="input">/WEB-INF/pages/quarters/addDept.jsp</result>
 		</action>
 		<!-- 宿舍管理人员维护 -->
 		<action name="listManager" class="org.king.quarters.web.action.ListManagerAction">
 			<result name="input">/WEB-INF/pages/quarters/managerList.jsp</result>
 		</action>
 		<action name="addOrUpdateManager" class="org.king.quarters.web.action.AddOrUpdateManagerAction">
 			<result name="input">/WEB-INF/pages/quarters/addManager.jsp</result>
 		</action>
   		 <!-- 宿舍资源维护 -->
 		<action name="listQuarters" class="org.king.quarters.web.action.ListQuartersAction">
 			<result name="input">/WEB-INF/pages/quarters/quartersList.jsp</result>
 			<result name="noRight">/WEB-INF/pages/quarters/noRight.jsp</result> 			
 		</action>
 		<action name="addOrUpdateQuarters" class="org.king.quarters.web.action.AddOrUpdateQuartersAction">
 			<result name="input">/WEB-INF/pages/quarters/addQuarters.jsp</result>
 		</action>
 		<action name="importQuarters" class="org.king.quarters.web.action.ImportQuartersAction">
 			<result name="input">/WEB-INF/pages/quarters/importQuarters.jsp</result>
 			<result name="noRight">/WEB-INF/pages/quarters/noRight.jsp</result>
 		</action>
 		<action name="listNoticeStudent" class="org.king.quarters.web.action.ListNoticeStudentAction">
 			<result name="input">/WEB-INF/pages/quarters/noticeStudentList.jsp</result>
 			<result name="noRight">/WEB-INF/pages/quarters/noRight.jsp</result>
 		</action>
 		<action name="printNotice" class="org.king.quarters.web.action.PrintNoticeAction">
 		    <!--北区 -->
 			<result name="8a70802613092cde0113093a30aa0003ruzhuNotice"  type="jasper">
 			    <param name="location">/WEB-INF/pages/quarters/beiqu_ruzhu_notice.jasper</param>
 			    <param name="dataSource">dataSource</param>
 			    <param name="format">RTF</param>
 			    <param name="contentDisposition">inline;filename=beiquruzhu.doc</param>
 			</result>
 			<result name="8a70802613092cde0113093a30aa0003jiaofeiNotice"  type="jasper">
 			    <param name="location">/WEB-INF/pages/quarters/beiqu_jiaokuan_notice.jasper</param>
 			    <param name="dataSource">dataSource</param>
 			    <param name="format">RTF</param>
 			    <param name="documentName">beiqujiaokuan.doc</param>
 			</result>
 			<result name="8a70802613092cde0113093a30aa0003juzhuNotice"  type="jasper">
 			    <param name="location">/WEB-INF/pages/quarters/beiqu_juzhu_xieyi.jasper</param>
 			    <param name="dataSource">dataSource</param>
 			    <param name="format">RTF</param>
 			    <param name="documentName">beiqujuzhuxieyi.doc</param>
 			</result>
 			<!--西苑 -->
 			<result name="8a708026132402840113240ad4710003ruzhuNotice">/WEB-INF/pages/quarters/noModel.jsp</result>
 			<result name="8a708026132402840113240ad4710003jiaofeiNotice">/WEB-INF/pages/quarters/noModel.jsp</result>
 			<result name="8a708026132402840113240ad4710003juzhuNotice">/WEB-INF/pages/quarters/noModel.jsp</result>
 			<!--张江 -->
 			<result name="ff8080811aec4488011aee206c6e00bfruzhuNotice">/WEB-INF/pages/quarters/noModel.jsp</result>
 			<result name="ff8080811aec4488011aee206c6e00bfjiaofeiNotice">/WEB-INF/pages/quarters/noModel.jsp</result>
 			<result name="ff8080811aec4488011aee206c6e00bfjuzhuNotice">/WEB-INF/pages/quarters/noModel.jsp</result>		
 		    <!--江湾 -->
 			<result name="ff8080811aec4488011aee209e2e00c1ruzhuNotice">/WEB-INF/pages/quarters/noModel.jsp</result>
 			<result name="ff8080811aec4488011aee209e2e00c1jiaofeiNotice">/WEB-INF/pages/quarters/noModel.jsp</result>
 			<result name="ff8080811aec4488011aee209e2e00c1juzhuNotice"  type="jasper">
 			    <param name="location">/WEB-INF/pages/quarters/jiangwan_juzhu_xieyi.jasper</param>
 			    <param name="dataSource">dataSource</param>
 			    <param name="format">RTF</param>
 			    <param name="documentName">jiangwanjuzhuxieyi.doc</param>
 			</result>
 		</action>
 		
    </package>    
           <package name="/summary" extends="mydefault">
    	<result-types>
