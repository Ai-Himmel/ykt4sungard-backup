[exportphoto]
exportsql=select cust.CUT_ID, cust.CUT_TYPE, cust.CUT_NAME, cust.AREA,cust.STUEMP_NO, cust.CLASSDEPT_NO, cust.S_CODE,ctype.TYPE_NAME,dept.DEPT_NAME,spec.S_NAME,photo.PHOTO_DATE ,photo.PHOTO_TIME from YKT_CUR.T_CIF_CUSTOMER cust   left join YKT_CUR.T_CIF_CUTTYPEFEE ctype on ctype.CUT_TYPE=cust.CUT_TYPE  left join YKT_CUR.T_CIF_DEPT dept on dept.DEPT_CODE=cust.CLASSDEPT_NO  left join YKT_CUR.T_CIF_SPECIALITY spec on spec.S_CODE=cust.S_CODE  left join YKT_CUR.T_CIF_PHOTO photo on photo.CUT_ID=cust.CUT_ID

[photostat]
photostatsql=select area.AREA_NAME,dept.DEPT_NAME, spec.S_NAME, count(cust.CUT_ID)as totnum, count(photo.photo_date) as photonum,count(cust.CUT_ID)-count(photo.photo_date) as unphotonum from ykt_cur.t_cif_photo photo   left join YKT_CUR.T_CIF_CUSTOMER cust on photo.cut_id=cust.cut_id   left join YKT_CUR.T_CIF_DEPT dept on cust.CLASSDEPT_NO=dept.DEPT_CODE  left join YKT_CUR.T_CIF_SPECIALITY spec on cust.S_CODE=spec.S_CODE  left join YKT_CUR.T_PIF_AREA area on cust.AREA=area.AREA_NO  where FATHER_AREA_NO=1