<?xml version="1.0"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="com.liferay.lock.service.spring.LockService.enterprise" class="com.liferay.portal.spring.RemoteSessionFactoryBean" lazy-init="true">
		<property name="businessInterface">
			<value>com.liferay.lock.service.spring.LockService</value>
		</property>
		<property name="jndiName">
			<value>com_liferay_lock_service_ejb_LockServiceEJB</value>
		</property>
	</bean>
	<bean id="com.liferay.lock.service.spring.LockServiceFactory" class="com.liferay.lock.service.spring.LockServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.lock.service.spring.LockService.enterprise" />
		</property>
	</bean>
</beans>