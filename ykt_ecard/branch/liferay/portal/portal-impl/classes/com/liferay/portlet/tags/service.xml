<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 4.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_4_4_0.dtd">

<service-builder package-path="com.liferay.portlet.tags">
	<namespace>Tags</namespace>
	<entity name="TagsAsset" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="assetId" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="classNameId" type="long" />
		<column name="classPK" type="long" />
		<column name="startDate" type="Date" />
		<column name="endDate" type="Date" />
		<column name="publishDate" type="Date" />
		<column name="expirationDate" type="Date" />
		<column name="mimeType" type="String" />
		<column name="title" type="String" />
		<column name="description" type="String" />
		<column name="summary" type="String" />
		<column name="url" type="String" />
		<column name="height" type="int" />
		<column name="width" type="int" />
		<column name="priority" type="double" />
		<column name="viewCount" type="int" />

		<!-- Relationships -->

		<column name="entries" type="Collection" entity="TagsEntry" mapping-table="TagsAssets_TagsEntries" />

		<!-- Finder methods -->

		<finder name="CompanyId" return-type="Collection">
			<finder-column name="companyId" />
		</finder>
		<finder name="C_C" return-type="TagsAsset">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>

		<!-- References -->

		<reference package-path="com.liferay.counter" entity="Counter" />
		<reference package-path="com.liferay.portal" entity="Company" />
		<reference package-path="com.liferay.portal" entity="Group" />
		<reference package-path="com.liferay.portal" entity="User" />
		<reference package-path="com.liferay.portlet.blogs" entity="BlogsEntry" />
		<reference package-path="com.liferay.portlet.bookmarks" entity="BookmarksEntry" />
		<reference package-path="com.liferay.portlet.documentlibrary" entity="DLFileEntry" />
		<reference package-path="com.liferay.portlet.journal" entity="JournalArticle" />
		<reference package-path="com.liferay.portlet.journal" entity="JournalArticleResource" />
		<reference package-path="com.liferay.portlet.messageboards" entity="MBMessage" />
		<reference package-path="com.liferay.portlet.wiki" entity="WikiPage" />
		<reference package-path="com.liferay.portlet.wiki" entity="WikiPageResource" />
	</entity>
	<entity name="TagsEntry" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="entryId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="name" type="String" />

		<!-- Relationships -->

		<column name="assets" type="Collection" entity="TagsAsset" mapping-table="TagsAssets_TagsEntries" />

		<!-- Order -->

		<order by="asc">
			<order-column name="name" />
		</order>

		<!-- Finder methods -->

		<finder name="C_N" return-type="TagsEntry">
			<finder-column name="companyId" />
			<finder-column name="name" />
		</finder>

		<!-- References -->

		<reference package-path="com.liferay.counter" entity="Counter" />
		<reference package-path="com.liferay.portal" entity="User" />
	</entity>
	<entity name="TagsProperty" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="propertyId" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="entryId" type="long" />
		<column name="key" type="String" />
		<column name="value" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="key" />
		</order>

		<!-- Finder methods -->

		<finder name="CompanyId" return-type="Collection">
			<finder-column name="companyId" />
		</finder>
		<finder name="EntryId" return-type="Collection">
			<finder-column name="entryId" />
		</finder>
		<finder name="C_K" return-type="Collection">
			<finder-column name="companyId" />
			<finder-column name="key" />
		</finder>
		<finder name="E_K" return-type="TagsProperty">
			<finder-column name="entryId" />
			<finder-column name="key" />
		</finder>

		<!-- References -->

		<reference package-path="com.liferay.counter" entity="Counter" />
		<reference package-path="com.liferay.portal" entity="User" />
	</entity>
	<entity name="TagsPropertyKey" local-service="false" remote-service="false" />
	<entity name="TagsSource" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="sourceId" type="long" primary="true" />

		<!-- Other fields -->

		<column name="parentSourceId" type="long" />
		<column name="name" type="String" />
		<column name="acronym" type="String" />
	</entity>
	<exceptions>
		<exception>DuplicateEntry</exception>
		<exception>EntryName</exception>
		<exception>PropertyKey</exception>
		<exception>PropertyValue</exception>
		<exception>TagsEntry</exception>
	</exceptions>
</service-builder>