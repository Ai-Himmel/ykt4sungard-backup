<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 4.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_4_4_0.dtd">

<service-builder package-path="com.liferay.portlet.test">
	<namespace>Test</namespace>
	<entity name="TestFoo" local-service="false" remote-service="false">

		<!-- PK fields -->

		<column name="fooId" type="Integer" primary="true" />

		<!-- Other fields -->

		<column name="field1" type="String" />
		<column name="field2" type="boolean" />

		<!-- Order -->

		<order by="asc">
			<order-column name="field1" />
			<order-column name="field2" />
		</order>
	</entity>
</service-builder>