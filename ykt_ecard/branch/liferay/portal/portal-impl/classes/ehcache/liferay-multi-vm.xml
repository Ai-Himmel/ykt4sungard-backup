<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd">

	<!--
	Caches in this file can be clustered but are not configured to be clustered.
	-->

	<diskStore path="user.home/liferay/ehcache/liferay-multi-vm" />
	<cacheManagerEventListenerFactory class="" properties="" />
	<defaultCache
		maxElementsInMemory="10000"
		eternal="false"
        timeToIdleSeconds="600"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portlet.alfrescocontent.util.AlfrescoContentCacheUtil"
		maxElementsInMemory="10000"
		eternal="false"
		timeToLiveSeconds="300"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portlet.calendar.service.impl.CalEventLocalUtil"
		maxElementsInMemory="10000"
		eternal="false"
        timeToIdleSeconds="600"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portal.cms.servlet.CMSServletUtil"
		maxElementsInMemory="10000"
		eternal="false"
        timeToIdleSeconds="600"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portal.spring.hibernate.FinderCache"
		maxElementsInMemory="10000"
		eternal="false"
        timeToIdleSeconds="600"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portlet.journalcontent.util.JournalContentUtil"
		maxElementsInMemory="1000"
		eternal="false"
		timeToLiveSeconds="3600"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portal.servlet.filters.layoutcache.LayoutCacheUtil"
		maxElementsInMemory="1000"
		eternal="false"
		timeToLiveSeconds="3600"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portal.security.permission.PermissionCacheUtil"
		maxElementsInMemory="10000"
		eternal="false"
        timeToIdleSeconds="600"
		overflowToDisk="true"
	/>
	<cache
		name="com.liferay.portal.service.impl.PortletPreferencesLocalUtil"
		maxElementsInMemory="10000"
		eternal="false"
        timeToIdleSeconds="600"
		overflowToDisk="true"
	/>
</ehcache>