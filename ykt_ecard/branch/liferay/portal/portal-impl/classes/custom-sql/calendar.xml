<?xml version="1.0"?>

<custom-sql>
	<sql id="com.liferay.portlet.calendar.service.persistence.CalEventFinder.findByG_SD">
		<![CDATA[
			SELECT
				{CalEvent.*}
			FROM
				CalEvent
			WHERE
				(groupId = ?) AND
				(startDate >= ?) AND
				(startDate <= ?) AND
				(timeZoneSensitive = ?) AND
				(repeating = ?)
		]]>
	</sql>
	<sql id="com.liferay.portlet.calendar.service.persistence.CalEventFinder.findByRemindBy">
		<![CDATA[
			SELECT
				{CalEvent.*}
			FROM
				CalEvent
			WHERE
				remindBy != ?
		]]>
	</sql>
</custom-sql>