<?xml version="1.0"?>

<project name="support-glassfish" basedir="." default="compile">
	<import file="../build-common-java.xml" />

	<property name="jar.file" value="${ant.project.name}" />

	<target name="compile">
		<unjar src="${project.dir}/lib/development/jbi_rt.jar" dest="classes">
			<patternset>
				<include name="**/JarFactory.class" />
			</patternset>
		</unjar>

		<antcall target="build-common-java.compile" />
	</target>

	<target name="jar">
	</target>

	<target name="deploy">
	</target>
</project>