<?xml version="1.0"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="com.liferay.documentlibrary.service.DLLocalService.impl" class="com.liferay.documentlibrary.service.impl.DLLocalServiceImpl" lazy-init="true" />
	<bean id="com.liferay.documentlibrary.service.DLLocalServiceFactory" class="com.liferay.documentlibrary.service.DLLocalServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.documentlibrary.service.DLLocalService.impl" />
		</property>
	</bean>
	<bean id="com.liferay.documentlibrary.service.DLService.impl" class="com.liferay.documentlibrary.service.impl.DLServiceImpl" lazy-init="true" />
	<bean id="com.liferay.documentlibrary.service.DLServiceFactory" class="com.liferay.documentlibrary.service.DLServiceFactory" lazy-init="true">
		<property name="service">
			<ref bean="com.liferay.documentlibrary.service.DLService.impl" />
		</property>
	</bean>
</beans>