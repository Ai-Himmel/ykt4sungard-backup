<?xml version="1.0"?>
<configuration>
    <appSettings>

        <!--  直连的DRTP 主机地址 10.253.42.200-->
        <add key="drtpHost" value="127.0.0.1"/>
        <!--DRTP 主机 的侦听端口号-->
        <add key="drtpPort" value="4000"/>
        <!--目标应用服务器连接的业务通讯平台节点编号 （参见业务通讯平台的配置参数表）-->
