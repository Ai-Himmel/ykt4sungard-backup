        <!--目标应用服务器注册在业务通讯平台的功能号(参见BCC配置文件中的BASEFUNCNO)-->
        <!--<add key="baseFunction" value="600"/>-->
        <add key="baseFunction" value="6000"/>
        <!--模仿的操作员号-->
        <add key="impersonate" value="1234"/>
    </appSettings>
