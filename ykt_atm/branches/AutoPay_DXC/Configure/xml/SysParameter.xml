<?xml version="1.0" encoding="gb2312"?>
<sysparameter>
	<!--设备编号-->
	<DeviceNo interval="65"/>
	<!--超时返回时间（分）-->
	<closetime interval="30"/>
	<!--屏保自动运行时间（分）-->
	<!--scrtype:1,播放Flash屏保，2,播放图片屏保-->
	<screentime interval="0" scrtype="2" flashname="defaultmovice.swf"/>
	<!--系统自动开关机控制 typevalue=0：自动关机；1：自动重启；2：持续工作-->
	<closetype typevalue="0" time="00:00:00"/>
	<!--取密钥延迟时间（秒）-->
	<GetKeyIntev interval="1"/>
	<!--心跳检测设置-->
	<moniter interval="60" ismon="1"/>
</sysparameter>
