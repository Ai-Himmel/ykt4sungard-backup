int CAccTrans::doTrans()
{
	int ret=0;
	if(!_pCfgTransCode)
	{
		_pCfgTransCode=getCfgTranscode(trans.transcode);
		if(!_pCfgTransCode)
			return ERRINFO(E_NOCFG_TRANSCODE,trans.transcode);
	}
	ret=doFeeTrans();
	if(ret)
		return ret;
	ret=doMainTrans();
	if(ret)
		return ret;
	return 0;
}
