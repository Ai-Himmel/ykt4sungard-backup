}
int GetHostSno(int *hostseqno)
{
	int ret=0;
	T_t_seqnoctl	tSeqnoctl;		
	memset(&tSeqnoctl,0,sizeof(tSeqnoctl));
	ret=DB_t_seqnoctl_read_lock_by_c0_and_termid(SEQNO_HOSTNO,&tSeqnoctl);
	if(ret)
	{
		if(DB_NOTFOUND==ret)
		{
			tSeqnoctl.termid=SEQNO_HOSTNO;
			tSeqnoctl.termseqno=1;
			ret=DB_t_seqnoctl_add(&tSeqnoctl);
			if(ret)
			{
				if(DB_REPEAT==ret)
					return E_DB_SEQNOCTL_E;
				else
					return E_DB_SEQNOCTL_I;
			}
			*hostseqno=tSeqnoctl.termseqno;
			return 0;
		}
		else
			return E_DB_SEQNOCTL_R;
	}
	++tSeqnoctl.termseqno;
	ret=DB_t_seqnoctl_update_lock_by_c0(&tSeqnoctl);
	if(ret)
	{
		if(DB_NOTFOUND==ret)
			return E_DB_SEQNOCTL_N;
		else
			return E_DB_SEQNOCTL_U;
	}
	*hostseqno=tSeqnoctl.termseqno;
	return 0;

}
int GetOperSeqno(int operid,int* operseqno)
{
	EXEC SQL BEGIN DECLARE SECTION;
	int hi_operid=0;
	int ho_operseqno=0;
	short idr;
	EXEC SQL END DECLARE SECTION;
	int ret=0;
	hi_operid=operid;
	EXEC SQL DECLARE oper_cur CURSOR FOR
		SELECT	operseqno 
		FROM t_operator
		WHERE operid = :hi_operid FOR UPDATE;
	if (SQLCODE)
	{
		ret = SQLCODE;
		db_chk_err(__FILE__,__LINE__,&sqlca);
		EXEC SQL CLOSE oper_cur;
		return E_DB_OPERATOR_R;
	}
	EXEC SQL OPEN oper_cur;
	if (SQLCODE)
	{
		ret = SQLCODE;
		db_chk_err(__FILE__,__LINE__,&sqlca);
		EXEC SQL CLOSE oper_cur;
		return E_DB_OPERATOR_R;
	}
	EXEC SQL FETCH oper_cur INTO
	:ho_operseqno:idr;
	if (SQLCODE)
	{
		ret = SQLCODE;
		db_chk_err(__FILE__,__LINE__,&sqlca);
		EXEC SQL CLOSE oper_cur;
		if(DB_NOTFOUND==ret)
			return ERRINFO(E_NOTEXIST_OPER);
		else
			return E_DB_OPERATOR_R;
	}
	++ho_operseqno;
	*operseqno=ho_operseqno;
	EXEC SQL UPDATE t_operator SET
		operseqno=:ho_operseqno
	WHERE current of oper_cur;
	if (SQLCODE)
	{
		ret = SQLCODE;
		db_chk_err(__FILE__,__LINE__,&sqlca);
		EXEC SQL CLOSE oper_cur;
		return E_DB_OPERATOR_U;
	}
	EXEC SQL CLOSE oper_cur;
	return 0;
}
int GetUniqSno(char *serialno,int type)
{
	int ret=0;
	char sysdate[9]={0};

	ret=db_getsysdate(sysdate);
	if(ret)
	{
		return ERRIF_DATABASE_DIS;
	}
	
	int iSysDate=atoi(sysdate);
	
	T_t_seqnoctl	tSeqnoctl;		
	memset(&tSeqnoctl,0,sizeof(tSeqnoctl));
	ret=DB_t_seqnoctl_read_lock_by_c0_and_termid(type,&tSeqnoctl);
	if(ret)
	{
		if(DB_NOTFOUND==ret)
		{
			int iSettleDate=0;
			ret=CAccTrans::GetInst().GetSettleDate(&iSettleDate);
			if(ret)
				return ret;
			if(iSettleDate>iSysDate)
			{
				writelog(LOG_ERR,"系统当前日期%d小于结算日期%d,请检查系统时间是否正确",iSysDate,iSettleDate);
				iSysDate = iSettleDate;
			}
			tSeqnoctl.termid=type;
			tSeqnoctl.termseqno=1;
			tSeqnoctl.accdate = iSysDate;
			ret=DB_t_seqnoctl_add(&tSeqnoctl);
			if(ret)
			{
				if(DB_REPEAT==ret)
					return E_DB_SEQNOCTL_E;
				else
					return E_DB_SEQNOCTL_I;
			}
			sprintf(serialno,"%08d%06d",iSysDate,tSeqnoctl.termseqno);
			return 0;
		}
		else
			return E_DB_SEQNOCTL_R;
	}
	if(tSeqnoctl.accdate>iSysDate)
	{
		writelog(LOG_ERR,"系统当前日期%d小于记账日期%d,请检查系统时间是否正确",iSysDate,tSeqnoctl.accdate);
		iSysDate = tSeqnoctl.accdate;
	}
	else if(tSeqnoctl.accdate<iSysDate)
	{
		//换日,重置流水号
		tSeqnoctl.accdate=iSysDate;
		tSeqnoctl.termseqno=0;
	}
	tSeqnoctl.termseqno++;
	if(tSeqnoctl.termseqno>999999)
	{
		writelog(LOG_ERR,"类型[%d]流水号超过系统最大值999999",type);
		return -1;
	}
	ret=DB_t_seqnoctl_update_lock_by_c0(&tSeqnoctl);
	if(ret)
	{
		if(DB_NOTFOUND==ret)
			return E_DB_SEQNOCTL_N;
		else
			return E_DB_SEQNOCTL_U;
	}
	sprintf(serialno,"%08d%06d",iSysDate,tSeqnoctl.termseqno);
	return 0;
}
