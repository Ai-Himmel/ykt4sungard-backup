int CAccTrans::LoadCfgSplit()
{
	EXEC SQL BEGIN DECLARE SECTION;
	int transcode=0;//交易码
	int transtype=0;//交易类型
	unsigned int drsubjno=0;//借方科目
	unsigned int crsubjno=0;//贷方科目
	int drflag=0;//是否核对借方明细科目
	int crflag=0;//是否核对贷方明细科目
	char drdesc[61]={0};//借方摘要
	char crdesc[61]={0};//贷方摘要
	short indicator=0;
	EXEC SQL END DECLARE SECTION;
	
	int ret = 0;
	int row = 0;
	int hashkey=0;
	T_t_cfgsplit tCfgSplit;

	if(mapSplit.size())
		mapSplit.clear();

	EXEC SQL DECLARE cfgsplit_cur CURSOR FOR
	SELECT 
		transcode,
		transtype,
		drsubjno,
		crsubjno,
		drflag,
		crflag,
		drdesc,
		crdesc
	FROM ykt_cur.t_cfgsplit 
	order by transcode;
	if(SQLCODE)
	{
	  CHECK_DB_ERR;
	  return E_DB_CURSOR_DECLARE;
	}
	EXEC SQL  OPEN cfgsplit_cur;
	if(SQLCODE)
	{
	  CHECK_DB_ERR;
	  return E_DB_CURSOR_OPEN;
	}
	while(1)
	{
		transcode=0;
		transtype=0;
		drsubjno=0;
		crsubjno=0;
		drflag=0;
		crflag=0;
		drdesc[0]=0;
		crdesc[0]=0;
		EXEC SQL FETCH cfgsplit_cur INTO
		:transcode:indicator,
		:transtype:indicator,
		:drsubjno:indicator,
		:crsubjno:indicator,
		:drflag:indicator,
		:crflag:indicator,
		:drdesc:indicator,
		:crdesc:indicator;
		if(SQLCODE)
		{
		  ret=SQLCODE;
		  CHECK_DB_ERR;
		  EXEC SQL CLOSE cfgsplit_cur;
		  if(DB_NOTFOUND==ret)
		  {
			  break;
		  }
		  else
			return E_DB_CFGSPLIT_R;
		}
		row++;
		memset(&tCfgSplit,0,sizeof(tCfgSplit));
		
		tCfgSplit.transcode = transcode;
		tCfgSplit.transtype = transtype;
		tCfgSplit.drsubjno=drsubjno;
		tCfgSplit.crsubjno=crsubjno;
		tCfgSplit.drflag = drflag;
		tCfgSplit.crflag = crflag;
		des2src(tCfgSplit.drdesc,drdesc);
		des2src(tCfgSplit.crdesc,crdesc);
		mapSplit[transcode]=tCfgSplit;
	}
	return 0;
}
//当lockflag不为false时,防止脏读
int CAccTrans::GetSettleDate(int *iSettleDate)
{
	int ret=0;
	if(sysPara.iBookSetNo<1)
	{
		ret=LoadSysPara();
		if(ret)
			return ret;
	}
	T_t_settlectl settlectl;
	memset(&settlectl,0,sizeof(settlectl));
	ret=DB_t_settlectl_read_by_booksetno(sysPara.iBookSetNo,&settlectl);
	if(ret)
	{
		if(DB_NOTFOUND==ret)
			return E_DB_SETTLECTRL_N;
		else
			return E_DB_SETTLECTRL_R;
	}
	if(settlectl.settleflag)
	{
		ERRTIP("当前系统正处于日终结算中");
		return ERRIF_SYS_DAYENDACC;
	}
	if(settlectl.settledate>settlectl.accdate)
	{
		ERRTIP("系统异常:结算日期%d大于当前记账日期%d",settlectl.settledate,settlectl.accdate);
		return E_COMMON_ERR;
	}
	accdate = settlectl.accdate;
	if(accdate!=hostdate)
	{
		if(accdate>hostdate)
		{
			writelog(LOG_ERR,"系统日期%d小于当前记账日期%d,请检查主机时钟",hostdate,accdate);
			//return E_COMMON_ERR;
		}
		else
		{
			accdate=hostdate;
		}
	}
	settledate=settlectl.settledate;
	if(iSettleDate)
		(*iSettleDate)=settledate;
	statdate = settlectl.statdate;
	batchdate= settlectl.batchdate;	
	return 0;
}
int CAccTrans::LoadSysPara()
{
	int ret;
	memset(&sysPara,0,sizeof(sysPara));
	T_t_actpara actpara;
	memset(&actpara,0,sizeof(actpara));
	ret=DB_t_actpara_read_by_paraid(1,&actpara);
	if(ret)
	{
	    if(DB_NOTFOUND==ret)
	        return E_DB_ACTPARA_N;
	    else
	        return E_DB_ACTPARA_R;
	}
	if(actpara.booksetno<1)
	{
		ERRTIP("当前帐套号不正确");
		return E_COMMON_ERR;
	}	
	T_t_bookset bookset;
	memset(&bookset,0,sizeof(bookset));

	ret=DB_t_bookset_read_by_booksetno(actpara.booksetno,&bookset);
	if(ret)
	{
	    if(DB_NOTFOUND==ret)
	        return E_DB_BOOKSET_N;
	    else
	        return E_DB_BOOKSET_R;
	}	
	if(bookset.enabledate<1)
	{
		ERRTIP("帐套未启用");
		return E_COMMON_ERR;
	}
	if(bookset.voucherclass<1)
	{
		ERRTIP("凭证类型未设置");
		return E_COMMON_ERR;
	}
	sysPara.iEnableDate=bookset.enabledate;
	sysPara.iSupervisor=bookset.supervisor;
	T_t_settlectl settlectl;
	memset(&settlectl,0,sizeof(settlectl));
	ret=DB_t_settlectl_read_by_booksetno(bookset.booksetno,&settlectl);
	if(ret)
	{
		if(DB_NOTFOUND==ret)
			return E_DB_SETTLECTRL_N;
		else
			return E_DB_SETTLECTRL_R;
	}
	if(settlectl.settledate<bookset.enabledate)
	{
		ERRTIP("业务日期[%d]初始化时设置错误,不能小于帐套启用日期%d",settlectl.settledate,bookset.enabledate);
		return E_COMMON_ERR;
	}
	settledate = settlectl.settledate;
	statdate = settlectl.statdate;
	batchdate = settlectl.batchdate;
	sysPara.iVoucherClass = bookset.voucherclass;
	sysPara.iBookSetNo= settlectl.booksetno;
	sysPara.iPeriodYear = settlectl.periodyear;
	sysPara.iPeriodMonth = settlectl.periodmonth;
	if(sysPara.iEnableDate==settledate)
		sysPara.iFirstDayFlag=1;
	return 0;
}
int CAccTrans::LoadCfgTransCode()
{
	EXEC SQL BEGIN DECLARE SECTION;
	sqlint32 transcode=0;//交易码
	char	 transname[61]="";//交易名称
	sqlint32 logflag=0;//是否记录日志
	sqlint32 limitflag=0;//是否限制系统操作员
	sqlint32 feeflag=0;//是否收费
	sqlint32 transflag=0;//是否交易
	sqlint32 disableflag=0;//是否禁用
	sqlint16 indicator=0;
	EXEC SQL END DECLARE SECTION;
	
	int ret = 0;
//	int row = 0;
	T_t_transcode tTransCode;
	if(mapCfgTransCode.size())
		mapCfgTransCode.clear();
	EXEC SQL DECLARE transcode_cur CURSOR FOR
	SELECT 
	  transcode,
	  transname,
	  logflag,
	  limitflag,
	  feeflag,
	  transflag,
	  disableflag
	FROM ykt_cur.t_transcode 
	order by transcode;
	if(SQLCODE)
	{
	  CHECK_DB_ERR;
	  return E_DB_CURSOR_DECLARE;
	}
	EXEC SQL  OPEN transcode_cur;
	if(SQLCODE)
	{
	  CHECK_DB_ERR;
	  return E_DB_CURSOR_OPEN;
	}
	while(1)
	{
		transcode=0;
		transname[0]=0;
		logflag=0;
		limitflag=0;
		feeflag=0;
		transflag=0;
		disableflag=0;
		EXEC SQL FETCH transcode_cur INTO
		:transcode:indicator,
		:transname:indicator,
		:logflag:indicator,
		:limitflag:indicator,
		:feeflag:indicator,
		:transflag:indicator,
		:disableflag:indicator;
		ret=SQLCODE;
		if(ret)
		{
		  CHECK_DB_ERR;
		  EXEC SQL CLOSE transcode_cur;
		  if(DB_NOTFOUND==ret)
			 break;
		  else
			return E_DB_TRANSCODE_R;
		}
		ret=SQLCODE;
		if(ret)
		{
		  CHECK_DB_ERR;
		  EXEC SQL CLOSE transcode_cur;
		  if(DB_NOTFOUND==ret)
			  break;
		  else
			return E_DB_TRANSCODE_R;
		}
		tTransCode.transcode=transcode;
		tTransCode.limitflag=limitflag;
		tTransCode.feeflag=feeflag;
		tTransCode.logflag=logflag;
		tTransCode.transflag=transflag;
		tTransCode.disableflag=disableflag;
		des2src(tTransCode.transname,transname);
		mapCfgTransCode[transcode]=tTransCode;
//		row++;
	}
	return 0;
}
