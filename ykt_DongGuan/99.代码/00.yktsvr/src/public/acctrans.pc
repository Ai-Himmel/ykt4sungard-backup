		if(remark.size()<=100)
			strcpy(tOperdtl.transdesc,remark.c_str());
	}
	ret=DB_t_operdtl_add(&tOperdtl);
	if(ret)
	{
	//	writelog(LOG_ERR,"DB_t_operdtl_add ret[%d]settledate[%d]termid[%d]termseqno[%d]",ret,tOperdtl.settledate,tOperdtl.termid,tOperdtl.termseqno);
		if(DB_REPEAT==ret)
			return E_DB_OPERDTL_E;
