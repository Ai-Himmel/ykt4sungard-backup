create table t_unit
(
  unitno number,
  unitname varchar2(60),
  enabled number,
  begindate number,
  lastdate number
)
begin
action modename ykt_cur end
action add end
action read_by unitno end
action update_by unitno end
action del_by unitno end
action read_lock_by c0 unitno end
action update_lock_by c0 end
action del_lock_by c0 end
action free_lock_by c0 end
action open_select_by c1 enabled end
action fetch_select_by c1 end
action close_select_by c1 end
end