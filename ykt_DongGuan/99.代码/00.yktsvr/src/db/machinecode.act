create table t_machinecode
(
  machinecode varchar(60),
  ip varchar(15),
  lastsaved varchar(30)
)
begin
action modename ykt_cur end
action add end
action read_by ip end
action read_by machinecode end
action update_by machinecode end
action del_by machinecode end
action del_by ip end
action read_lock_by c0 machinecode end
action update_lock_by c0 end
action free_lock_by c0 end
end
