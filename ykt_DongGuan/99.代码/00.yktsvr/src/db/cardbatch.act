create table t_cardbatch
(
  batchno varchar2(14),
  cardnum number,
  crttime varchar2(14),
  syndate number,
  startdate number,
  enddate number,
  status number,
  updtime varchar2(14)
)
begin
action modename ykt_cur end
action add end
action read_by batchno end
action update_by batchno end
action del_by batchno end
action read_lock_by c0 batchno end
action update_lock_by c0 end
action del_lock_by c0 end
action free_lock_by c0 end
end