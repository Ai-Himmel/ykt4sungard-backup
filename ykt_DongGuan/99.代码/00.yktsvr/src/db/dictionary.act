create table t_dictionary
(
  dicttype integer,
  dicttypename varchar(60),
  dictval varchar(30),
  dictcaption varchar(60)
)
begin
action modename ykt_cur end
action add end
action read_by dicttype dictval end
action update_by dicttype dictval end
action del_by dicttype dictval end
action read_lock_by c0 dicttype dictval end
action update_lock_by c0 end
action free_lock_by c0 end
end
