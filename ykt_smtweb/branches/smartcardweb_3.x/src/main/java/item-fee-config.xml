<?xml version="1.0" encoding="UTF-8"?>
<item-config xmlns="http://kingstar.com/itemconfig">
	<item>
	    <!-- 唯一标示符，不同的收费项目，必须有不同的id -->
	    <gid>1</gid>
		<!-- 在多媒体机上是否显示这个收费条目 -->
		<display>true</display>
		<!-- 收费的项目名称 -->
		<name>GS缴费</name>
		<!--商户编号， 该收费项目缴费到哪个商户的编号-->
		<shop-id>1</shop-id>
		<!-- 收费金额 -->
		<fee>
			<!-- 收费类型，enumeration枚举类型 , input 表示手动输入 -->
			<type>input</type>
			
		</fee>
		<!-- 收费项目的开始时间，如果系统时间不在开始时间和结束时间范围内，该收费项目显示为灰色，即不可点击 -->
		<start-time>2009-10-10</start-time>
		<!-- 结束时间 -->
		<end-time>2020-10-20</end-time>
		<!-- 业务处理 -->
		<business-processor>
			<!-- 处理相应业务的具体实现类 -->
			<classname>cn.edu.dlmu.GsProcessor</classname>
			<!-- 向处理器传递的参数值 -->
			<param>
				<property>serviceURL</property>
				<value></value>
			</param>
			<param>
				<property>serviceName</property>
				<value></value>
			</param>
		</business-processor>
		<description>GS缴费</description>
	</item>
</item-config>
