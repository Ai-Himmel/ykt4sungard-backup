create table t_site
(
  siteno integer,
  sitename varchar(60),
  ip varchar(15),
  machinecode varchar(60),
  remark    varchar(240),
  lastsaved varchar(30),
)
begin
action modename ykt_cur end
action add end
action read_by siteno end
action read_by ip end
action update_by siteno end
action del_by siteno end
action read_lock_by c0 siteno end
action update_lock_by c0 end
action free_lock_by c0 end
end
