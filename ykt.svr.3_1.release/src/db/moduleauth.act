create table t_moduleauth
(
  modulecode varchar(30),
  modulename varchar(60),
  menuset varchar2(800),
  funcset varchar2(600),
  opercode varchar(8),
  lastsaved varchar(30)
)
begin
action modename ykt_cur end
action add end
action read_by modulecode end
action update_by modulecode end
action del_by modulecode end
action read_lock_by c0 modulecode end
action update_lock_by c0 end
action free_lock_by c0 end
end
