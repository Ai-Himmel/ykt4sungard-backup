create table t_cardbook
(
  showcardno varchar(10),
  cardtype number,
  cardphyid varchar2(16),
  opercode varchar2(8),
  putopercode varchar2(8),
  batchno varchar2(30),
  getdate char(8),
  usedate char(8),
  reclaimdate char(8),
  usestatus number(1)
)
begin
action modename ykt_cur end
action add end
action read_by showcardno end
action update_by showcardno end
action del_by showcardno end
action read_lock_by c0 showcardno end
action update_lock_by c0 end
action free_lock_by c0 end
end
