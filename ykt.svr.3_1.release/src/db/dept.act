create table t_dept
(
  deptcode varchar(30),
  areacode char(2),
  deptname varchar(480),
  deptename varchar(240),
  deptfullname varchar(1000),
  fdeptcode varchar(30),
  deptlevel integer,
  olddeptcode varchar(30),
  useflag char(1),
  lastsaved varchar(30)
)
begin
action modename ykt_cur end
action add end
action read_by deptcode end
action update_by deptcode end
action del_by deptcode end
action read_lock_by c0 deptcode end
action update_lock_by c0 end
action free_lock_by c0 end
end
